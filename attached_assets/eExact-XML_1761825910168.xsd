				<xsd:element name="JobLevelTo" type="xsd:integer" minOccurs="0"/>
				<xsd:element name="JobCode" type="xsd:string" minOccurs="0"/>
				<xsd:element ref="JobGroup" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute ref="code"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="JobGroups">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="JobGroup" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="JobGroup">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Description" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute ref="code"/>
		</xsd:complexType>
