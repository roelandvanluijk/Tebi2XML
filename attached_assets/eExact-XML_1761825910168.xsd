								</xsd:complexType>
							</xsd:element>
							<xsd:element name="ForeignCurrency" minOccurs="0">
								<xsd:complexType>
									<xsd:attribute name="code" type="xsd:string" use="required"/>
									<xsd:attribute name="value" type="xsd:float" use="required"/>
									<xsd:attribute name="exchangerate" type="xsd:float"/>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="OpportunityStage" minOccurs="0"/>
				<xsd:element name="CloseDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="Probability" type="xsd:float" minOccurs="0"/>
				<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
				<xsd:element name="ActionDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="NextAction" type="xsd:string" minOccurs="0"/>
				<xsd:element ref="LeadSource" minOccurs="0"/>
				<xsd:element ref="ReasonCode" minOccurs="0"/>
				<xsd:element ref="Project" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="number" type="xsd:integer" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Quotation">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="OrderedBy" type="typeAccount"/>
				<xsd:element name="OrderContact" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence minOccurs="0">
							<xsd:element name="LastName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="FirstName" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
						<xsd:attribute name="ID" type="xsd:string" use="required"/>
						<xsd:attribute name="HID" type="xsd:integer" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="InvoiceTo" type="typeAccount"/>
				<xsd:element name="InvoiceContact" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence minOccurs="0">
							<xsd:element name="LastName" type="xsd:string" minOccurs="0"/>
							<xsd:element name="FirstName" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
						<xsd:attribute name="ID" type="xsd:string" use="required"/>
						<xsd:attribute name="HID" type="xsd:integer" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element name="QuotationDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="DueDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="ClosingDate" type="xsd:date" minOccurs="0"/>
				<xsd:element ref="ForeignAmount" minOccurs="1"/>
				<xsd:element ref="PaymentCondition" minOccurs="0"/>
				<xsd:element ref="Opportunity" minOccurs="0"/>
				<xsd:element ref="YourRef" minOccurs="0"/>
				<xsd:element name="SalesPerson" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence minOccurs="0">
							<xsd:element name="FullName" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
						<xsd:attribute name="id" type="xsd:string" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="SalesChannel" type="typeSalesChannel" minOccurs="0"/>
				<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
				<xsd:element ref="EntryDiscount" minOccurs="0"/>
				<xsd:element name="QuotationLine" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Description" minOccurs="0"/>
							<xsd:element ref="Item"/>
							<xsd:element name="Notes" type="xsd:string"/>
							<xsd:element ref="Description" minOccurs="0"/>
							<xsd:element ref="StartDate" minOccurs="0"/>
							<xsd:element ref="EndDate" minOccurs="0"/>
							<xsd:element ref="Quantity" minOccurs="0"/>
							<xsd:element name="UnitPrice" type="typePrice" minOccurs="0"/>
							<xsd:element name="DiscountPercentage" type="typePercentage" minOccurs="0"/>
							<xsd:element name="NetPrice" type="typePrice" minOccurs="0"/>
							<xsd:element ref="Unit" minOccurs="0"/>
							<xsd:element ref="ForeignAmount" minOccurs="0"/>
							<xsd:element ref="Costcenter" minOccurs="0"/>
							<xsd:element ref="Costunit" minOccurs="0"/>
							<xsd:element name="Optional" type="xsd:boolean" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>
										Indicates the optional line
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
						<xsd:attribute name="line" type="xsd:integer" use="optional"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="CloseDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="Source" type="xsd:integer" minOccurs="1"/>
				<xsd:element ref="Project" minOccurs="0"/>
				<xsd:element ref="ShippingMethod" minOccurs="0"/>
				<xsd:element name="DeliveryDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="Incoterm" type="typeIncoterm" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="quotationnumber" type="xsd:integer" use="required"/>
			<xsd:attribute name="status" type="xsd:integer" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="LeadStages">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="LeadStage" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="LeadStage">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element name="SortOrder" type="xsd:integer" minOccurs="0"/>
				<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
				<xsd:element name="LeadStatus" type="xsd:integer" minOccurs="0"/>
				<xsd:element ref="Active" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="code" type="xsd:string" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="LeadRatings">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="LeadRating" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="LeadRating">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
				<xsd:element ref="Active" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="code" type="xsd:string" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Leads">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Lead" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="LeadContact">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="FirstName" type="xsd:string" minOccurs="0"/>
				<xsd:element name="MiddleName" type="xsd:string" minOccurs="0"/>
				<xsd:element name="LastName" type="xsd:string" minOccurs="0"/>
				<xsd:element name="FullName" type="xsd:string" minOccurs="0"/>
				<xsd:element name="Initials" type="xsd:string" minOccurs="0"/>
				<xsd:element name="Title" type="xsd:string" minOccurs="0"/>
				<xsd:element name="Gender" type="xsd:string" minOccurs="0"/>
				<xsd:element name="Nationality" type="xsd:string" minOccurs="0"/>
				<xsd:element name="Language" type="xsd:string" minOccurs="0"/>
				<xsd:element name="BirthDate" type="xsd:string" minOccurs="0"/>
				<xsd:element name="Phone" type="xsd:string" minOccurs="0"/>
				<xsd:element name="PhoneExtension" type="xsd:string" minOccurs="0"/>
				<xsd:element name="Mobile" type="xsd:string" minOccurs="0"/>
				<xsd:element name="E-mail" type="xsd:string" minOccurs="0"/>
				<xsd:element name="JobTitleDescription" type="xsd:string" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Lead">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="LeadContact" minOccurs="0"/>
				<xsd:element name="General" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="LeadStatus" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Description" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
									<xsd:attribute name="ID" type="xsd:integer" use="required"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element ref="LeadStage" minOccurs="0"/>
							<xsd:element name="Owner" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence minOccurs="0">
										<xsd:element name="FullName" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
									<xsd:attribute name="username" type="xsd:string" use="required"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element ref="LeadRating" minOccurs="0"/>
							<xsd:element ref="LeadSource" minOccurs="0"/>
							<xsd:element name="Amount" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence minOccurs="0">
										<xsd:element name="DefaultCurrency" minOccurs="0">
											<xsd:complexType>
												<xsd:attribute name="code" type="xsd:string"/>
												<xsd:attribute name="value" type="xsd:float" use="required"/>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="ForeignCurrency" minOccurs="0">
											<xsd:complexType>
												<xsd:attribute name="code" type="xsd:string" use="required"/>
												<xsd:attribute name="value" type="xsd:float" use="required"/>
												<xsd:attribute name="exchangerate" type="xsd:float"/>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="DueDate" type="xsd:date" minOccurs="0"/>
							<xsd:element name="CloseDate" type="xsd:date" minOccurs="0"/>
							<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Company" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence minOccurs="0">
							<xsd:element name="Name" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Fax" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Website" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="VisitAddress" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence minOccurs="0">
							<xsd:element name="Line1" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Line2" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Line3" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Postcode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="City" type="xsd:string" minOccurs="0"/>
							<xsd:element name="State" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Country" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="MailAddress" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence minOccurs="0">
							<xsd:element name="Line1" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Line2" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Line3" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Postcode" type="xsd:string" minOccurs="0"/>
							<xsd:element name="City" type="xsd:string" minOccurs="0"/>
							<xsd:element name="State" type="xsd:string" minOccurs="0"/>
							<xsd:element name="Country" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Account" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence minOccurs="0">
							<xsd:element name="Name" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
						<xsd:attribute name="ID" type="xsd:string" use="required"/>
						<xsd:attribute ref="code" use="optional"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="Opportunity" minOccurs="0"/>
				<xsd:element name="Contact" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence minOccurs="0">
							<xsd:element name="PersonID" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
						<xsd:attribute name="ID" type="xsd:string" use="required"/>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="number" type="xsd:integer" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<!-- Sales Force Automation - end -->
	<!-- Currency - Start-->
	<xsd:element name="ExchangeRates">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ExchangeRate" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ExchangeRate">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="AdministrationCurrency"/>
				<xsd:element ref="ForeignCurrency"/>
				<xsd:element ref="Date"/>
				<xsd:element name="Rate" type="xsd:float"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="AdministrationCurrency" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>
				This is the default currency of the Administration.
				Rate will be calculated as (AdministrationCurrency) = (ExchangeRate) * (ForeignCurrency)
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ForeignCurrency" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>
				This is the currency whose value will be calculated in terms of the default currency of the Administration.
				Rate will be calculated as (AdministrationCurrency) = (ExchangeRate) * (ForeignCurrency)
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- Currency - End -->
	<!-- TimeEntry start-->
	<xsd:element name="TimeTransaction">
		<xsd:annotation>
			<xsd:documentation>Time entries can be used when the Time &amp; Billing module is in the subscription</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Account" minOccurs="0"/>
				<xsd:element ref="Date"/>
				<xsd:element ref="Employee"/>
				<xsd:element ref="Item"/>
				<xsd:element ref="Note" minOccurs="0"/>
				<xsd:element ref="Project" minOccurs="0"/>
				<xsd:element name ="Activity" minOccurs="0">
					<xsd:complexType>
						<xsd:attribute name="ID" type="xsd:string" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="Quantity"/>
				<xsd:element name="Price" type="typePrice" minOccurs="0"/>
				<xsd:element ref="Attachment" minOccurs="0"/>
				<xsd:element ref="Subscription" minOccurs="0" />
			</xsd:sequence>
			<xsd:attribute name="number" type="xsd:integer" use="optional">
				<xsd:annotation>
					<xsd:documentation>Unique number indentifying the entry</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="status" use="optional">
				<xsd:annotation>
					<xsd:documentation>
						Status codes for import:
						1=Draft,
						10=Submitted (only applicable if administration setting 'Approve: entries' is enabled).

						Status codes for export:
						1=Draft,
						-1=Submitted,
						2=Rejected,
						-2=Contains errors,
						-3=Approved.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="10"/>
						<xsd:enumeration value="11"/>
						<xsd:enumeration value="14"/>
						<xsd:enumeration value="16"/>
						<xsd:enumeration value="19"/>
						<xsd:enumeration value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="TimeTransactions">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="TimeTransaction" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- TimeEntry end-->
	<!-- CostEntry start-->
	<xsd:element name="CostTransaction">
		<xsd:annotation>
			<xsd:documentation>Cost entries can be used when the Time &amp; Billing module is in the subscription</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Account" minOccurs="0"/>
				<xsd:element ref="Date"/>
				<xsd:element ref="Employee"/>
				<xsd:element ref="Item"/>
				<xsd:element ref="Note" minOccurs="0"/>
				<xsd:element ref="Project" minOccurs="0"/>
				<xsd:element name ="Expense" minOccurs="0">
					<xsd:complexType>
						<xsd:attribute name="ID" type="xsd:string" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="Quantity"/>
				<xsd:element name="Price" type="typePrice" minOccurs="0"/>
				<xsd:element ref="Attachment" minOccurs="0"/>
				<xsd:element ref="Subscription" minOccurs="0" />
			</xsd:sequence>
			<xsd:attribute name="number" use="optional">
				<xsd:annotation>
					<xsd:documentation>Unique number indentifying the entry</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="status" use="optional">
				<xsd:annotation>
					<xsd:documentation>
						1=Draft,
						2=Rejected,
						10=Submitted,
						11=Failed on approval,
						14=Waiting to be approved,
						16=Waiting to be unapproved,
						19=Failed on unapproval,
						20=Final.

						Only status 1 (Draft) and 10 (Submitted) are available for import,
						but status 10 (Submitted) can only be used when the administration setting 'Approve: entries' is checked.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="10"/>
						<xsd:enumeration value="11"/>
						<xsd:enumeration value="14"/>
						<xsd:enumeration value="16"/>
						<xsd:enumeration value="19"/>
						<xsd:enumeration value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="CostTransactions">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="CostTransaction" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- CostEntry end-->
	<!-- ProjectTerm start-->
	<xsd:element name="ProjectInvoiceTerm">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="Description" type="xsd:string" minOccurs="0"/>
				<xsd:element name="Amount" type="xsd:float" minOccurs="1"/>
				<xsd:element name="InvoiceDate" type="xsd:date" minOccurs="1"/>
				<xsd:element ref="VAT" minOccurs="0"/>
				<xsd:element name="Notes" type="xsd:string" minOccurs="0" />
				<xsd:element name="Item" type="typeItem" minOccurs="0"/>
				<xsd:element name="ExecutionFromDate" type="xsd:date" minOccurs="1"/>
				<xsd:element name="ExecutionToDate" type="xsd:date" minOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute name="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ProjectInvoiceTerms">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ProjectInvoiceTerm" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- ProjectTerm end-->
	<!-- ProjectBudgetedHourType start-->
	<xsd:element name="ProjectBudgetedHourType">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="HourType" type="typeItem" minOccurs="1"/>
				<xsd:element name="BudgetedHours" type="xsd:float" minOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute name="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ProjectBudgetedHourTypes">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ProjectBudgetedHourType" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- ProjectBudgetedHourType end-->
	<!--Project start -->
	<xsd:element name="Project">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="Description" type="xsd:string"/>
				<xsd:element ref="Translations" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="Account" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="StartDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="EndDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="ProjectManager" type="typeUser" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
				<xsd:element name="InternalNotes" type="xsd:string" minOccurs="0"/>
				<xsd:element name="BudgetedTime" type="xsd:float" minOccurs="0"/>
				<xsd:element name="BudgetedAmount" type="xsd:float" minOccurs="0"/>
				<xsd:element name="AlertTime" type="xsd:float" minOccurs="0"/>
				<xsd:element name="FixedPriceItem" type="typeItem" minOccurs="0"/>
				<xsd:element ref="ProjectClassification" minOccurs="0"/>
				<xsd:element name="CustomerPONumber" type="xsd:string" minOccurs="0"/>
				<xsd:element name="PrepaidType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>1=Retainer, 2=Hour type bundle</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="PrepaidItem" type="typeItem" minOccurs="0"/>
				<xsd:element name="MarkupPercentage" type="xsd:float" minOccurs="0"/>
				<xsd:element name="BudgetedCosts" type="xsd:float" minOccurs="0"/>
				<xsd:element name="BudgetedRevenue" type="xsd:float" minOccurs="0"/>
				<xsd:element ref="Items" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element name="RebillableItems" type="typeItems"  minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="Employees" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="EmployeeItems" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="ProjectInvoiceTerms" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="ProjectBudgetedHourTypes" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element name="BlockEntry" type="xsd:boolean" minOccurs="0"/>
				<xsd:element name="BlockPlanning" type="xsd:boolean" minOccurs="0"/>
				<xsd:element name="BlockRebilling" type="xsd:boolean" minOccurs ="0"/>
				<xsd:element name="BlockPurchasing" type="xsd:boolean" minOccurs ="0"/>
				<xsd:element name="BlockInvoicing" type="xsd:boolean" minOccurs ="0"/>
				<xsd:element name="BudgetType" minOccurs ="0"/>
				<xsd:element name="BudgetOverrunHours" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>10=Allowed, 20=Not Allowed</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="20"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="InvoiceAsQuoted" type="xsd:boolean" minOccurs="0"/>
				<xsd:element ref="SourceQuotation" minOccurs="0"/>
				<xsd:element name="UseBillingMilestones" type="xsd:boolean" minOccurs="0"/>
				<xsd:element name="AllowAdditionalInvoices" type="xsd:boolean" minOccurs="0"/>
				<xsd:element name="IncludeInvoiceSpecification" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>1=Based on account, 2=Always, 3=Never</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:integer">
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
							<xsd:enumeration value="3"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="IncludeSpecificationInInvoicePdf" type="xsd:boolean" minOccurs ="0"/>
				<xsd:element name="AllowMemberEntryOnly" type="xsd:boolean" minOccurs ="0"/>
				<xsd:element name="PaymentCondition" minOccurs="0"/>
				<xsd:element name="IsWBSRequiredForEntry" minOccurs ="0">
					<xsd:annotation>
						<xsd:documentation>0 = Based on company setting, 1 = Yes, 2 = No</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:integer">
							<xsd:enumeration value="0"/>
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="EnableWorkInProgress" type="xsd:boolean" minOccurs ="0"/>
			</xsd:sequence>
			<xsd:attribute ref="code" use="required">
				<xsd:annotation>
					<xsd:documentation>Unique number indentifying the entry</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="type" use="optional">
				<xsd:annotation>
					<xsd:documentation>1=Campaign, 2=Fixed price, 3=Time and Material, 4=Non billable, 5=Prepaid</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
						<xsd:enumeration value="4"/>
						<xsd:enumeration value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Projects">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Project" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ProjectClassification">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="Description" type="xsd:string"/>
				<xsd:element ref="Translations" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="Costcenter" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="Costunit" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="UseEmployeeCostCenter" type="xsd:boolean" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute ref="code" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ProjectClassifications">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ProjectClassification" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="EmployeeItems">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="EmployeeItem" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="EmployeeItem">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Employee" minOccurs="0" />
				<xsd:element ref="Item" minOccurs="0" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!--Project end-->

	<!--Project SourceQuotation start -->
	<xsd:element name="SourceQuotation">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Description" type="xsd:string" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="EntryID" type="xsd:string" use="required"/>
			<xsd:attribute name="Number" type="xsd:integer" use="optional"/>
			<xsd:attribute name="Version" type="xsd:integer" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<!--Project SourceQuotation end -->

	<!--Project Manufacturing starts-->
	<xsd:element name="ManufacturingProject">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="Description" type="xsd:string"/>
				<xsd:element name="StartDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="EndDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute ref="code" use="required">
				<xsd:annotation>
					<xsd:documentation>Unique number indentifying the entry</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ManufacturingProjects">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ManufacturingProject" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!--Project Manufacturing end-->

	<!-- ProjectWBS start-->
	<!--Deliverable -->
	<xsd:element name="Deliverable">
		<xsd:complexType mixed="true">
			<xsd:sequence>
				<xsd:element name="Description" type="xsd:string"/>
				<xsd:element name="Milestone" type="xsd:boolean" minOccurs="0"/>
				<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
				<xsd:choice minOccurs="0" maxOccurs="unbounded">
					<xsd:element ref="Activity"/>
					<xsd:element ref="Expense"/>
					<xsd:element ref="Deliverable"/>
				</xsd:choice>
			</xsd:sequence>
			<xsd:attribute name="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>

	<!-- Activity -->
	<xsd:element name="Activity">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Description" type="xsd:string"/>
				<xsd:element name="Completed" type="xsd:boolean" minOccurs="0"/>
				<xsd:element name="Hours" type="xsd:decimal" minOccurs="0"/>
				<xsd:element name="HourAlert" type="xsd:decimal" minOccurs="0"/>
				<xsd:element name="DefaultHourType" type="typeItem"  minOccurs="0"/>
				<xsd:element name="DefaultHourTypeIsMandatory" type="xsd:boolean" minOccurs="0"/>
				<xsd:element name="BudgetOverrunHours" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>10=Allowed, 20=Not Allowed</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="20"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="InvoiceMethod" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>2=Fixed price, 3=Time and Material, 4=Non billable, 5=Prepaid</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:integer">
							<xsd:enumeration value="2"/>
							<xsd:enumeration value="3"/>
							<xsd:enumeration value="4"/>
							<xsd:enumeration value="5"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="InvoiceDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="InvoiceTermId" type="xsd:string" minOccurs="0"/>
				<xsd:element name="AutoCreateInvoiceTerm" type="xsd:boolean" minOccurs="0"/>
				<xsd:element name="BlockEntry" type="xsd:boolean" minOccurs="0"/>
				<xsd:element name="StartDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="EndDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="CostsAmount" type="xsd:decimal" minOccurs="0"/>
				<xsd:element name="RevenueAmount" type="xsd:decimal" minOccurs="0"/>
				<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>

	<!-- Expense -->
	<xsd:element name="Expense">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Description" type="xsd:string"/>
				<xsd:element name="BlockRebilling" type="xsd:boolean" minOccurs ="0"/>
				<xsd:element name="DefaultCostType" type="typeItem" minOccurs="0"/>
				<xsd:element name="Quantity" type="xsd:decimal" minOccurs="0"/>
				<xsd:element name="Supplier" type="typeAccount" minOccurs="0"/>
				<xsd:element name="PurchasePrice" type="xsd:decimal" minOccurs="0"/>
				<xsd:element name="InvoiceMethod" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>6=Fixed, 7=Rebill, 8=Non re-billable</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:integer">
							<xsd:enumeration value="6"/>
							<xsd:enumeration value="7"/>
							<xsd:enumeration value="8"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="InvoiceDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="InvoiceTermId" type="xsd:string" minOccurs="0"/>
				<xsd:element name="AutoCreateInvoiceTerm" type="xsd:boolean" minOccurs="0"/>
				<xsd:element name="BlockEntry" type="xsd:boolean" minOccurs="0"/>
				<xsd:element name="StartDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="EndDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="CostsAmount" type="xsd:decimal" minOccurs="0"/>
				<xsd:element name="RevenueAmount" type="xsd:decimal" minOccurs="0"/>
				<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="WorkBreakdownStructure">
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="Deliverable"/>
				<xsd:element ref="Activity"/>
				<xsd:element ref="Expense"/>
			</xsd:choice>
			<xsd:attribute name="Project" type="xsd:string" use="required"/>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="WorkBreakdownStructures">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="WorkBreakdownStructure" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!--ProjectWBS end-->

	<!-- WBSTemplate start-->
	<!--TypeDeliverable -->
	<xsd:complexType name="TypeDeliverable">
		<xsd:sequence>
			<xsd:element name="Description" type="xsd:string"/>
			<xsd:element name="Milestone" type="xsd:boolean" minOccurs="0"/>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="Activity" type="TypeActivity"/>
				<xsd:element name="Deliverable" type="TypeDeliverable"/>
				<xsd:element name="Expense" type="TypeExpense"/>
			</xsd:choice>
		</xsd:sequence>
		<xsd:attribute name="ID" use="optional"/>
	</xsd:complexType>

	<!--TypeActivity -->
	<xsd:complexType name="TypeActivity">
		<xsd:sequence>
			<xsd:element name="Description" type="xsd:string"/>
			<xsd:element name="Hours" type="xsd:decimal" minOccurs="0"/>
			<xsd:element name="HourAlert" type="xsd:decimal" minOccurs="0"/>
			<xsd:element name="DefaultHourType" type="typeItem" minOccurs="0"/>
			<xsd:element name="BudgetOverrunHours" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>10=Allowed, 20=Not Allowed</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="10"/>
						<xsd:enumeration value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="BlockEntry" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="CostsAmount" type="xsd:decimal" minOccurs="0"/>
			<xsd:element name="RevenueAmount" type="xsd:decimal" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="ID" use="optional"/>
	</xsd:complexType>

	<!--TypeExpense -->
	<xsd:complexType name="TypeExpense">
		<xsd:sequence>
			<xsd:element name="Description" type="xsd:string"/>
			<xsd:element name="BlockRebilling" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="BlockEntry" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="CostsAmount" type="xsd:decimal" minOccurs="0"/>
			<xsd:element name="RevenueAmount" type="xsd:decimal" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="ID" use="optional"/>
	</xsd:complexType>

	<xsd:element name="WBSTemplate">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Description" type ="xsd:string"/>
				<xsd:choice minOccurs="0" maxOccurs="unbounded">
					<xsd:element name="Deliverable" type="TypeDeliverable"/>
					<xsd:element name="Activity" type="TypeActivity"/>
					<xsd:element name="Expense" type ="TypeExpense"/>
				</xsd:choice>
			</xsd:sequence>
			<xsd:attribute name="code" type="xsd:string" use="required"/>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="WBSTemplates">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="WBSTemplate" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- WBSTemplate end-->

	<!-- ProjectPlanning start -->
	<xsd:element name="ProjectPlanningLine">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Account" minOccurs="0"/>
				<xsd:element ref="Project" minOccurs="0"/>
				<xsd:element name="Activity" minOccurs="0">
					<xsd:complexType>
						<xsd:attribute name="ID" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Description" type="xsd:string"/>
				<xsd:element name="StartDate" type="xsd:date"/>
				<xsd:element name="EndDate" type="xsd:date"/>
				<xsd:element ref="Item"/>
				<xsd:element ref="Quantity" minOccurs="0"/>
				<xsd:element name="OverAllocate" type="xsd:boolean" minOccurs="0"/>
				<xsd:element ref="Employee" />
				<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="status" use="required">
				<xsd:annotation>
					<xsd:documentation>1:Reservation, 2:Planning</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="ProjectPlanning">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ProjectPlanningLine" maxOccurs="unbounded" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- ProjectPlanning end -->

	<!--SubscriptionType start -->
	<xsd:element name="SubscriptionTypes">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="SubscriptionType" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SubscriptionType">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Description"/>
				<xsd:element ref="Active"/>
				<xsd:element name="AutomaticProlongation" type="xsd:integer"/>
				<xsd:element name="ManualRenewalMethod" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>1=Use item prices, 2=Use current subscription prices</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:integer">
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="CancellationPeriod" type="TypeSubscriptionPeriod" minOccurs="1"/>
				<xsd:element name="ExtCancellationPeriod" type="TypeSubscriptionPeriod" minOccurs="1"/>
				<xsd:element name="SubscriptionPeriod" type="TypeSubscriptionPeriod" minOccurs="1"/>
				<xsd:element name="ExtSubscriptionPeriod" type="TypeSubscriptionPeriod" minOccurs="1"/>
				<xsd:element name="InvoicePeriod" type="TypeSubscriptionPeriod" minOccurs="1"/>
				<xsd:element name="InvoiceCorrectionMethod" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>1=Ratio based, 2=Zero Invoice, 3=Never invoiced</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:integer">
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
							<xsd:enumeration value="3"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element ref="Note" minOccurs="0"/>
				<xsd:element ref="SubscriptionLayout" minOccurs="0"/>
				<xsd:element ref="CoveringLetterLayout" minOccurs="0"/>
				<xsd:element name="AutoGenerateInvoiceType" type="TypeAutoGenerateInvoice" minOccurs="0"/>
				<xsd:element name="AutoSendInvoiceType" type="TypeAutoSendInvoice" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute ref="code" use="required">
				<xsd:annotation>
					<xsd:documentation>Unique number indentifying the entry</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="TypeSubscriptionPeriod">
		<xsd:sequence>
			<xsd:element name="PeriodUnit" type="xsd:string"/>
		</xsd:sequence>
		<xsd:attribute name="period" use="required" type="xsd:integer"/>
	</xsd:complexType>
	<xsd:element name="SubscriptionLayout">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Document" maxOccurs="1"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="CoveringLetterLayout">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Document" maxOccurs="1"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="TypeAutoGenerateInvoice">
		<xsd:sequence>
			<xsd:element name="AutoGenerateInvoiceDays" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="AutoGenerateInvoiceDescription" type="typeDescription" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="type" use="optional" type="xsd:integer"/>
	</xsd:complexType>
	<xsd:complexType name="TypeAutoSendInvoice">
		<xsd:sequence>
			<xsd:element name="AutoSendInvoiceDays" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="AutoSendInvoiceMethod" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>1=Send based on account, 2=Send via Email, 3=Create documents, 4=Send via digital postbox, 5=Send and track</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
						<xsd:enumeration value="4"/>
						<xsd:enumeration value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="EnablePaymentLink" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>0=Never, 1=Always, 2=Based on account</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="0"/>
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AutoSendInvoiceSender" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="AutoSendInvoiceSenderMailbox" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="type" use="optional" type="xsd:integer"/>
	</xsd:complexType>
	<!--Subscription Type end-->
	<!--Subscription start -->
	<xsd:element name="Subscription">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Description" type="xsd:string"/>
				<xsd:element ref="SubscriptionType" minOccurs="0"/>
				<xsd:element ref="StartDate" minOccurs="0"/>
				<xsd:element ref="EndDate" minOccurs="0"/>
				<xsd:element name="CancellationDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="InvoicingStartDate" type="xsd:date" minOccurs="0"/>
				<xsd:element name="InvoiceDay" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
						For monthly, quaterly, half-year and yearly invoice period, the day number of the month when the invoice of the subscription is sent.
						While for weekly invoice period, the weekday number of the week when the invoice of the subscription is sent:
						0=Monday
						1=Tuesday
						2=Wednesday
						3=Thursday
						4=Friday
						5=Saturday
						6=Sunday
						</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:integer">
							<xsd:minInclusive value="0" />
							<xsd:maxInclusive value="31" />
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="OrderedBy" type="typeAccount" minOccurs="0"/>
				<xsd:element name="InvoicedTo" type="typeAccount" minOccurs="0"/>
				<xsd:element ref="Contact" minOccurs="0"/>
				<xsd:element ref="PaymentCondition" minOccurs="0"/>
				<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
				<xsd:element name="CustomerPONumber" type="xsd:string" minOccurs="0"/>
				<xsd:element ref="SubscriptionClassification" minOccurs="0"/>
				<xsd:element ref="SubscriptionReasonCancelled" minOccurs="0"/>
				<xsd:element name="BlockEntry" type="xsd:boolean" minOccurs="0"/>
				<xsd:element ref="Items" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="Employees" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element name="Currency" type="typeCurrency" minOccurs="0"/>
				<xsd:element ref="Project" minOccurs="0"/>
				<xsd:element name="SubscriptionLine" minOccurs="1" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Description"/>
							<xsd:element ref="Item"/>
							<xsd:element ref="Quantity" />
							<xsd:element ref="Unit" />
							<xsd:element name="UnitPrice" type="xsd:float" minOccurs="0"/>
							<xsd:element name="NetPrice" type="xsd:float" minOccurs="0"/>
							<xsd:element ref="VAT" minOccurs="0"/>
							<xsd:element name="DiscountPercentage" type="typePercentage" minOccurs="0"/>
							<xsd:element ref="StartDate" minOccurs="0"/>
							<xsd:element ref="EndDate" minOccurs="0"/>
							<xsd:element ref="Costcenter" minOccurs="0"/>
							<xsd:element ref="Costunit" minOccurs="0"/>
							<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
							<xsd:element name="LineType" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence minOccurs="0">
										<xsd:element ref="Description"/>
									</xsd:sequence>
									<xsd:attribute name="ID" use="required">
										<xsd:annotation>
											<xsd:documentation>1=Recurring, 2=OneTime</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:token">
												<xsd:enumeration value="1"/>
												<xsd:enumeration value="2"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
						<xsd:attribute name="line" type="xsd:integer" use="optional"/>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="Number" use="required" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>Unique number indentifying the entry</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="entryID" use="optional" type="xsd:string" />
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Subscriptions">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Subscription" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SubscriptionClassification">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Description"/>
			</xsd:sequence>
			<xsd:attribute ref="code" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SubscriptionReasonCancelled">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Description"/>
			</xsd:sequence>
			<xsd:attribute ref="code" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<!--Subscription end-->
	<!--HRMSubscription start -->
	<xsd:element name="HRMSubscriptions">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="HRMSubscription" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="HRMSubscription">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="GeneralData" type="TypeGeneralData" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="ContractData" type="TypeContractData" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="CompanyData" type="TypeCompanyData" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="PersonalData" type="TypePersonalData" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="LoginData" type="TypeLoginData" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="TypeGeneralData">
		<xsd:sequence>
			<xsd:element name="PartnerKey" type="xsd:string"/>
			<xsd:element name="Country" type="xsd:string"/>
			<xsd:element name="Language" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TypeContractData">
		<xsd:sequence>
			<xsd:element name="Type" type="xsd:string"/>
			<xsd:element name="FixedPackage" type="xsd:string"/>
			<xsd:element name="FixedTerm" type="xsd:string"/>
			<xsd:element name="ExtraModules" type="xsd:integer"/>
			<xsd:element name="Template" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TypeCompanyData">
		<xsd:sequence>
			<xsd:element name="Accountant" type="xsd:string"/>
			<xsd:element name="Reseller" type="xsd:string"/>
			<xsd:element name="CompanyName" type="xsd:string"/>
			<xsd:element name="Address" type="xsd:string"/>
			<xsd:element name="PostalCode" type="xsd:string"/>
			<xsd:element name="City" type="xsd:string"/>
			<xsd:element name="ChamberOfCommerce" type="xsd:string"/>
			<xsd:element name="PhoneNumber" type="xsd:string"/>
			<xsd:element name="Website" type="xsd:string"/>
			<xsd:element name="VATNumber" type="xsd:string"/>
			<xsd:element name="Email" type="xsd:string"/>
			<xsd:element name="Bank" type="xsd:string"/>
			<xsd:element name="BankAccount" type="xsd:string"/>
			<xsd:element name="BankAccountHolder" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TypePersonalData">
		<xsd:sequence>
			<xsd:element name="Title" type="xsd:string"/>
			<xsd:element name="FirstName" type="xsd:string"/>
			<xsd:element name="MiddleName" type="xsd:string"/>
			<xsd:element name="LastName" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TypeLoginData">
		<xsd:sequence>
			<xsd:element name="UserName" type="xsd:string"/>
			<xsd:element name="Password" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<!--HRMSubscription end -->
	<!--StockPositions start -->
	<xsd:element name="StockPosition">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="CurrentQuantity" type="xsd:float"/>
				<xsd:element name="Planning-In" type="xsd:float"/>
				<xsd:element name="Planning-Out" type="xsd:float"/>
				<xsd:element ref="Item"/>
				<xsd:element ref="Warehouse" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute ref="ID"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="StockPositions">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="StockPosition" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!--StockPositions end -->
	<!--StorageLocationStockPositions start -->
	<xsd:element name="StorageLocationStockPosition">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="CurrentQuantity" type="xsd:float"/>
				<xsd:element ref="Item" minOccurs="0"/>
				<xsd:element ref="Warehouse" minOccurs="1"/>
				<xsd:element ref="StorageLocation" minOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute ref="ID"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="StorageLocationStockPositions">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="StorageLocationStockPosition" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!--StorageLocationStockPositions end -->
	<!--Warehouse start -->
	<xsd:element name="Warehouse">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="Description" minOccurs="0" type="xsd:string"/>
				<xsd:element name="AddressLine1" minOccurs="0" type="xsd:string"/>
				<xsd:element name="AddressLine2" minOccurs="0" type="xsd:string"/>
				<xsd:element name="AddressLine3" minOccurs="0" type="xsd:string"/>
				<xsd:element name="Postcode" minOccurs="0" type="xsd:string"/>
				<xsd:element name="City" minOccurs="0" type="xsd:string"/>
				<xsd:element ref="State" minOccurs="0"/>
				<xsd:element ref="Country" minOccurs="0"/>
				<xsd:element name="Manager" type="typeUser" minOccurs="0"/>
				<xsd:element name="Phone" minOccurs="0" type="xsd:string"/>
				<xsd:element name="PhoneExt" minOccurs="0" type="xsd:string"/>
				<xsd:element name="Fax" minOccurs="0" type="xsd:string"/>
				<xsd:element name="Email" minOccurs="0" type="xsd:string"/>
				<xsd:element name="Main" minOccurs="0" type="xsd:boolean"/>
				<xsd:element name="UseStorageLocations" minOccurs="0" type="xsd:boolean"/>
				<xsd:element name="StorageLocations" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="StorageLocation" minOccurs="0" maxOccurs="unbounded"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="WarehouseItems" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="ItemWarehouse" minOccurs="0" maxOccurs="unbounded"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute ref="code" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<!--Warehouse end -->
	<!--StorageLocation start -->
	<xsd:element name="StorageLocation" type="typeStorageLocation">
		<xsd:annotation>
			<xsd:documentation>A location in a warehouse.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="typeStorageLocation">
		<xsd:sequence>
			<xsd:element name="Description" minOccurs="0" type="xsd:string"/>
		</xsd:sequence>
		<xsd:attribute ref="code" use="required"/>
	</xsd:complexType>
	<!--StorageLocation end -->
	<!--StockCounts start -->
	<xsd:element name="StockCount">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="Description" minOccurs="0" type="xsd:string"/>
				<xsd:element name="TransactionDate" minOccurs="0" type="xsd:date"/>
				<xsd:element ref="GLAccount" minOccurs="0"/>
				<xsd:element ref="Warehouse" minOccurs="0"/>
				<xsd:element name="StockCountLine" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence minOccurs="1" maxOccurs="1">
							<xsd:element name="Quantity" type="xsd:float"/>
							<xsd:element name="NewQuantity" type="xsd:float"/>
							<xsd:element ref="Item"/>
							<xsd:element ref="Unit" minOccurs="0"/>
							<xsd:element ref="StorageLocation" minOccurs="0" maxOccurs="1"/>
							<xsd:choice minOccurs="0" maxOccurs="1">
								<xsd:element ref="SerialNumbers" />
								<xsd:element ref="BatchNumbers" />
							</xsd:choice>
							<xsd:element ref="ReasonLogistics" minOccurs="0" maxOccurs="1"/>
						</xsd:sequence>
						<xsd:attribute name="LineNumber" type="xsd:integer"/>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="StockCountNumber" type="xsd:integer"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="StockCounts">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="StockCount" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!--StockCounts end -->
	<!--Warehouses start -->
	<xsd:element name="Warehouses">
		<xsd:annotation>
			<xsd:documentation>Warehouses are used if you have the multiwarehouse functionality in your subscription</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Warehouse" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!--Warehouses end -->
	<xsd:element name="WarehouseTransfer">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element name="Remark" type="xsd:string" minOccurs="0"/>
				<xsd:element name="WarehouseFrom" minOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Description"/>
						</xsd:sequence>
						<xsd:attribute name="Code" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="WarehouseTo" minOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Description"/>
						</xsd:sequence>
						<xsd:attribute name="Code" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="EntryDate" type="xsd:date" minOccurs="0" />
				<xsd:element name="PlannedTransferDate" type="xsd:date" minOccurs="0" />
				<xsd:element name="PlannedReceiptDate" type="xsd:date" minOccurs="0" />
				<xsd:element name="WarehouseTransferLine" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Description" minOccurs="0"/>
							<xsd:element ref="Item" minOccurs="1"/>
							<xsd:element ref="Quantity" minOccurs="1"/>
							<xsd:element ref="Unit" minOccurs="0"/>
							<xsd:element name="StorageLocationFrom" type="typeStorageLocation" minOccurs="0" maxOccurs="1" />
							<xsd:element name="StorageLocationTo"  type="typeStorageLocation" minOccurs="0"  maxOccurs="1" />
						</xsd:sequence>
						<xsd:attribute name="Line" type="xsd:integer" use="optional"/>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="TransferNumber" use="optional">
				<xsd:annotation>
					<xsd:documentation>The transfer number can be used multiple times.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="Status" use="optional">
				<xsd:annotation>
					<!--WarehouseTransferStatuses.Open, WarehouseTransferStatuses.Partial, WarehouseTransferStatuses.Complete-->
					<xsd:documentation>10=Open, 30=Partial, 50=Complete</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="10"/>
						<xsd:enumeration value="30"/>
						<xsd:enumeration value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="WarehouseTransfers">
		<xsd:annotation>
			<xsd:documentation>WarehouseTransfers are used if you have the warehouse transfer functionality in your subscription</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="WarehouseTransfer" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!--AssemblyOrders start -->
	<xsd:element name="AssemblyOrder">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Warehouse" minOccurs="1"/>
				<xsd:element ref="Item" minOccurs="1"/>
				<xsd:element name="PlannedQuantity" minOccurs="1" type="xsd:float"/>
				<xsd:element name="FinishedQuantity" minOccurs="0" type="xsd:float">
					<xsd:annotation>
						<xsd:documentation>Only used for export, imported assembly orders always in Open status</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Description" minOccurs="0" type="xsd:string"/>
				<xsd:element name="OrderDate" minOccurs="0" type="xsd:date">
					<xsd:annotation>
						<xsd:documentation>Order date: YYYY-MM-DD</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="AssemblyDate" minOccurs="0" type="xsd:date">
					<xsd:annotation>
						<xsd:documentation>Assembly date: YYYY-MM-DD</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="StorageLocation" minOccurs="0"/>
				<xsd:element name="Notes" minOccurs="0" type="xsd:string"/>
				<xsd:element ref="AssemblyPartItems" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Only used for export</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="ordernumber" use="optional">
				<xsd:annotation>
					<xsd:documentation>The order number can be used multiple times. If an assembly order with this order number already exists, a new assembly order with the same order number will be created. When no order number is supplied, a new assembly order with the next available order number will be created.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="status" use="optional">
				<xsd:annotation>
					<xsd:documentation>20=Open, 30=Partial, 50=Complete. Only used for export, imported assembly orders always get status 20.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="20"/>
						<xsd:enumeration value="30"/>
						<xsd:enumeration value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="type" use="optional">
				<xsd:annotation>
					<xsd:documentation>8060=Assembly, 8061=Disassemble. Only used for export, imported assembly orders always default according to assembly order type.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="8060"/>
						<xsd:enumeration value="8061"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="AssemblyOrders">
		<xsd:annotation>
			<xsd:documentation>AssemblyOrders are used if you have the Assembly Order functionality in your subscription</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="AssemblyOrder" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="AssemblyPartItem">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Item" minOccurs="0"/>
				<xsd:element ref="StorageLocation" minOccurs="0"/>
				<xsd:element name="PlannedQuantity" minOccurs="0" type="xsd:float"/>
				<xsd:element name="Issued" minOccurs="0" type="xsd:float"/>
				<xsd:element name="Unit" minOccurs="0" type="xsd:string"/>
				<xsd:element name="Notes" minOccurs="0" type="xsd:string"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="AssemblyPartItems">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="AssemblyPartItem" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!--AssemblyOrders end -->
	<!--FinishedAssemblies start -->
	<xsd:element name="FinishedAssemblies">
		<xsd:annotation>
			<xsd:documentation>FinishedAssemblies are used if you have the Assembly Order functionality in your subscription</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="FinishedAssembly" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="FinishedAssembly">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="AssemblyDate" minOccurs="0" type="xsd:date">
					<xsd:annotation>
						<xsd:documentation>Assembly date: YYYY-MM-DD</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Quantity" minOccurs="0" type="xsd:float"/>
				<xsd:choice minOccurs="0" maxOccurs="1">
					<xsd:element ref="SerialNumbers" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="BatchNumbers" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:choice>
			</xsd:sequence>
			<xsd:attribute name="ordernumber" use="optional">
				<xsd:annotation>
					<xsd:documentation>The order number can be used multiple times.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="assemblynumber" use="optional">
				<xsd:annotation>
					<xsd:documentation>Only used for export.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="type" use="optional">
				<xsd:annotation>
					<xsd:documentation>8060=Assembly, 8061=Disassemble. Only used for export, imported assembly orders always default type according PlannedQuantity.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="8060"/>
						<xsd:enumeration value="8061"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Parts">
		<xsd:complexType>
			<xsd:sequence maxOccurs="unbounded">
				<xsd:element ref="Part" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Part">
		<xsd:complexType>
			<xsd:sequence maxOccurs="unbounded">
				<xsd:element ref="Item"/>
				<xsd:element ref="StorageLocation" minOccurs="0" maxOccurs="1"/>
				<xsd:choice minOccurs="0" maxOccurs="1">
					<xsd:element ref="SerialNumbers" />
					<xsd:element ref="BatchNumbers" />
				</xsd:choice>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!--FinishedAssemblies end -->
	<!--SalesForecasts start-->
	<xsd:element name="SalesForecasts">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="SalesForecast" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SalesForecast">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Description" minOccurs="1"/>
				<xsd:element name="Warehouse" minOccurs="1" maxOccurs="1">
					<xsd:complexType>
						<xsd:attribute ref="code" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="YearPeriodFrom" minOccurs="1" />
				<xsd:element ref="YearPeriodTo" minOccurs="1" />
				<xsd:element name="Notes" type="xsd:string" minOccurs="0" />
				<xsd:element ref="SalesForecastItems" minOccurs="0" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute name="code" type="xsd:string" use="required"/>
			<xsd:attribute name="periodType" use="required">
				<xsd:annotation>
					<xsd:documentation>2=Week, 3=Month</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="isActive" use="required">
				<xsd:annotation>
					<xsd:documentation>1=Active, 0=Inactive</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="YearPeriodFrom">
		<xsd:complexType>
			<xsd:attribute name="year" type="xsd:integer" use="required">
				<xsd:annotation>
					<xsd:documentation>The minimum year is the first financial year used for the financial periods. out of range=year lesser than the minimum year</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="period" type="xsd:integer" use="required">
				<xsd:annotation>
					<xsd:documentation>If periodType=2, min=1 max=52/53(if year has 53 weeks). If periodType=3, min=1 max=12</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="YearPeriodTo">
		<xsd:complexType>
			<xsd:attribute name="year" type="xsd:integer" use="required">
				<xsd:annotation>
					<xsd:documentation>The maximum year is 10 years from the current year. out of range=year greater than the maximum year</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="period" type="xsd:integer" use="required">
				<xsd:annotation>
					<xsd:documentation>If periodType=2, min=1 max=52/53(if year has 53 weeks). If periodType=3, min=1 max=12</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SalesForecastItems">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="SalesForecastItem" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SalesForecastItem">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Item" minOccurs="1" maxOccurs="unbounded"/>
				<xsd:element ref="SalesForecastDetails" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SalesForecastDetails">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="SalesForecastDetail" minOccurs="1" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SalesForecastDetail">
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="xsd:double">
					<xsd:attribute name="year" type="xsd:integer" use="required" />
					<xsd:attribute name="period" type="xsd:integer" use="required">
						<xsd:annotation>
							<xsd:documentation>If periodType=2, min=1 max=52/53(if year has 53 weeks). If periodType=3, min=1 max=12</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<!--SalesForecasts end -->
	<!--Deliveries start-->
	<xsd:element name="Deliveries">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Delivery" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Delivery">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Account" maxOccurs="1"/>
				<xsd:element ref="DeliveryAddress" minOccurs="0"/>
				<xsd:element ref="Contact" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="ShippingMethod" minOccurs="0"/>
				<xsd:choice minOccurs="1" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>
							SplitDeliveryLine=MultipleStorageLocationSplitting featureset,
							DeliveryLine= else
						</xsd:documentation>
					</xsd:annotation>
					<xsd:element ref="DeliveryLine" minOccurs="1" maxOccurs="unbounded"/>
					<xsd:element ref="SplitDeliveryLine" minOccurs="1" maxOccurs="unbounded"/>
				</xsd:choice>
			</xsd:sequence>
			<xsd:attribute name="Description" use="optional" type="xsd:string"/>
			<xsd:attribute name="EntryDate" use="optional" type="xsd:date"/>
			<xsd:attribute name="DeliveryNumber" use="optional" type="xsd:string"/>
			<xsd:attribute name="TrackingNumber" use="optional" type="xsd:string"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="DeliveryLine">
		<xsd:complexType>
			<xsd:sequence maxOccurs="1">
				<xsd:element ref="Item"/>
				<xsd:element ref="StorageLocation" minOccurs="0" maxOccurs="1"/>
				<xsd:choice minOccurs="0" maxOccurs="1">
					<xsd:element ref="SerialNumbers" />
					<xsd:element ref="BatchNumbers" />
				</xsd:choice>
			</xsd:sequence>
			<xsd:attribute name="SalesOrderNumber" use="required" type="xsd:integer"/>
			<xsd:attribute name="SalesOrderLine" use="optional" type="xsd:integer"/>
			<xsd:attribute name="Quantity" use="required" type="xsd:float"/>
			<xsd:attribute name="DeliveryDate" use="optional" type="xsd:date"/>
			<xsd:attribute name="TrackingNumber" use="optional" type="xsd:string"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SplitDeliveryLine">
		<xsd:complexType>
			<xsd:sequence maxOccurs="1">
				<xsd:element ref="Item"/>
				<xsd:element ref="StockTransactionLocations" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute name="SalesOrderNumber" use="required" type="xsd:integer"/>
			<xsd:attribute name="SalesOrderLine" use="optional" type="xsd:integer"/>
			<xsd:attribute name="Quantity" use="required" type="xsd:float"/>
			<xsd:attribute name="DeliveryDate" use="optional" type="xsd:date"/>
			<xsd:attribute name="TrackingNumber" use="optional" type="xsd:string"/>
		</xsd:complexType>
	</xsd:element>
	<!--Deliveries end-->
	<!--Receipts  start-->
	<xsd:element name="Receipts">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Receipt" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Receipt">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:choice minOccurs="1" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>
							SplitReceiptLine=MultipleStorageLocationSplitting featureset,
							ReceiptLine= else
						</xsd:documentation>
					</xsd:annotation>
					<xsd:element ref="ReceiptLine" minOccurs="1" maxOccurs="unbounded"/>
					<xsd:element ref="SplitReceiptLine" minOccurs="1" maxOccurs="unbounded"/>
				</xsd:choice>
			</xsd:sequence>
			<xsd:attribute name="Description" use="optional" type="xsd:string"/>
			<xsd:attribute name="EntryDate" use="optional" type="xsd:date"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ReceiptLine">
		<xsd:complexType>
			<xsd:sequence maxOccurs="1">
				<xsd:element ref="Item"/>
				<xsd:element ref="StorageLocation" minOccurs="0" maxOccurs="1"/>
				<xsd:choice minOccurs="0" maxOccurs="1">
					<xsd:element ref="SerialNumbers" />
					<xsd:element ref="BatchNumbers" />
				</xsd:choice>
			</xsd:sequence>
			<xsd:attribute name="PurchaseOrderNumber" use="required" type="xsd:integer"/>
			<xsd:attribute name="PurchaseOrderLine" use="optional" type="xsd:integer"/>
			<xsd:attribute name="Quantity" use="required" type="xsd:float"/>
			<xsd:attribute name="ReceiptDate" use="optional" type="xsd:date"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SplitReceiptLine">
		<xsd:complexType>
			<xsd:sequence maxOccurs="1">
				<xsd:element ref="Item"/>
				<xsd:element ref="StockTransactionLocations" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute name="PurchaseOrderNumber" use="required" type="xsd:integer"/>
			<xsd:attribute name="PurchaseOrderLine" use="optional" type="xsd:integer"/>
			<xsd:attribute name="Quantity" use="required" type="xsd:float"/>
			<xsd:attribute name="ReceiptDate" use="optional" type="xsd:date"/>
		</xsd:complexType>
	</xsd:element>
	<!--Receipts end-->
	<!--Entry Templates Start-->
	<xsd:element name="EntryTemplates">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="EntryTemplate" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="EntryTemplate">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Journal" minOccurs="1"/>
				<xsd:element name="JournalType" minOccurs="1" type="xsd:integer"/>
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element ref="PaymentCondition" minOccurs="0"/>
				<xsd:element name="Currency" type="typeCurrency" minOccurs="0"/>
				<xsd:element ref="Note" minOccurs="0"/>
				<xsd:element name="Reversal" type="xsd:boolean" minOccurs="0"/>
				<xsd:element name="Blocked" type="xsd:boolean" minOccurs="1"/>
				<xsd:element ref="EntryTemplateLine" minOccurs="1" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attribute ref="code" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="EntryTemplateLine">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="GLAccount" type="typeGLAccount" minOccurs="1"/>
				<xsd:element ref="Note" minOccurs="0"/>
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element ref="Costcenter" minOccurs="0"/>
				<xsd:element ref="Costunit" minOccurs="0"/>
				<xsd:element ref="Project" minOccurs="0"/>
				<xsd:element ref="Quantity" minOccurs="0"/>
				<xsd:element ref="VAT" minOccurs="0"/>
				<xsd:element name="AmountCalculation" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation>0=No Value, 1=Fixed Value, 2=Relative weight</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="0"/>
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Weight" type="xsd:positiveInteger" minOccurs="0"/>
				<xsd:element name="Amount" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>This amount is in the currency of the header.</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Value" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="LineNumber" type="xsd:integer" use="required"/>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="EntryTemplateLinks">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="EntryTemplateLink" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="EntryTemplateLink">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="EntryTemplate" minOccurs="1"/>
				<xsd:element name="EntityType" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation>1=Account</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="1"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element ref="Account" minOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute name="Main" type="xsd:boolean" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<!--Entry Templates-->
	<xsd:element name="EntryTemplateTransactions">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="EntryTemplateTransaction" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="EntryTemplateTransaction">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="EntryTemplate" minOccurs="1"/>
				<xsd:element ref="Account" minOccurs="1"/>
				<xsd:element name="EntryDate" type="xsd:date" minOccurs="1"/>
				<xsd:element name="Amount" minOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Value" minOccurs="1"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="Document" minOccurs="0"/>
				<xsd:element name="Status" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation>20=Open, 50=Processed</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="20"/>
							<xsd:enumeration value="50"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element ref="YourRef" minOccurs="0"/>
				<xsd:element name="PaymentReference" minOccurs="0"/>
				<xsd:element ref="OrderNumber" minOccurs="0"/>
				<xsd:element name="Entry" type="xsd:integer" minOccurs="0"/>
				<xsd:element ref="FinYear" minOccurs="0"/>
				<xsd:element ref="Journal" minOccurs="0"/>
				<xsd:element name="Currency" type="typeCurrency" minOccurs="1"/>
				<xsd:element ref="Description" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!--Entry Templates End-->
	<!--Manufacturing Types (Private to schema) -->
	<xsd:complexType name="SupplierReference">
		<xsd:attribute ref="ID" use="optional"/>
		<xsd:attribute ref="code" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="ItemReference">
		<xsd:attribute ref="ID" use="optional"/>
		<xsd:attribute ref="code" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="WorkcenterResource">
		<xsd:sequence>
			<xsd:element name="Setup" type="xsd:float"/>
			<xsd:element name="SetupCount" type="xsd:int"/>
			<xsd:element name="SetupUnit">
				<xsd:annotation>
					<xsd:documentation>hh=Hour, mi=Minute, ss=Second</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="hh"/>
						<xsd:enumeration value="mi"/>
						<xsd:enumeration value="ss"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Run" type="xsd:float"/>
			<xsd:element name="RunMethod">
				<xsd:annotation>
					<xsd:documentation>10=Fixed hours, 20=Fixed minutes, 25=Fixed days, 30=Hours/Item, 40=Minutes/Item, 50=Seconds/Item, 60=Items/Hour, 70=Items/Minute</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="10"/>
						<xsd:enumeration value="20"/>
						<xsd:enumeration value="25"/>
						<xsd:enumeration value="30"/>
						<xsd:enumeration value="40"/>
						<xsd:enumeration value="50"/>
						<xsd:enumeration value="60"/>
						<xsd:enumeration value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="EfficiencyPercentage" type="xsd:float"/>
			<xsd:element name="AttendedPercentage" type="xsd:float"/>
			<xsd:element name="PlannedSetupHours" type="xsd:float"/>
			<xsd:element name="PlannedRunHours" type="xsd:float"/>
		</xsd:sequence>
		<xsd:attribute ref="ID" use="optional"/>
		<xsd:attribute ref="code" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="SupplierResource">
		<xsd:sequence>
			<xsd:element name="PurchaseLeadDays" type="xsd:int"/>
			<xsd:element name="MinimumCharge" type="xsd:float" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="PurchaseUnit" type="xsd:string"/>
			<xsd:element name="PurchaseVATCode" type="xsd:string" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
		<xsd:attribute ref="ID" use="optional"/>
		<xsd:attribute ref="code" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="ShopOrderReference">
		<xsd:attribute ref="ID" use="optional"/>
		<xsd:attribute ref="code" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="OperationReference">
		<xsd:attribute ref="ID" use="optional"/>
		<xsd:attribute ref="code" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="WorkcenterReference">
		<xsd:attribute ref="ID" use="optional"/>
		<xsd:attribute ref="code" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="OperationResourceReference">
		<xsd:sequence>
			<xsd:element name="Operation" type="OperationReference"/>
			<xsd:choice>
				<xsd:element name="Workcenter" type="WorkcenterReference"/>
				<xsd:element name="Supplier" type="SupplierReference"/>
			</xsd:choice>
		</xsd:sequence>
		<xsd:attribute ref="ID" use="optional"/>
	</xsd:complexType>
	<xsd:element name="CalculatorType">
		<xsd:annotation>
			<xsd:documentation>0=Fixed, 1=Materials per piece, 2=Pieces per material, 3=Pieces per bar, 4=Pieces per sheet, 5=Length per piece, 6=Weight per piece, 7=Volume per piece, 8=Phantom, 9=Phantom (zero lead time)</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType >
			<xsd:restriction base="xsd:token">
				<xsd:enumeration value="0"/>
				<xsd:enumeration value="1"/>
				<xsd:enumeration value="2"/>
				<xsd:enumeration value="3"/>
				<xsd:enumeration value="4"/>
				<xsd:enumeration value="5"/>
				<xsd:enumeration value="6"/>
				<xsd:enumeration value="7"/>
				<xsd:enumeration value="8"/>
				<xsd:enumeration value="9"/>
			</xsd:restriction>
		</xsd:simpleType >
	</xsd:element>

	<!--Manufacturing Unit of Measure Types-->
	<xsd:simpleType name="UnitOfMeasure">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1" />
			<xsd:maxLength value="16" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="WeightUnitOfMeasure"  >
		<xsd:annotation>
			<xsd:documentation>Unit of measure code, whose type must be weight</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="UnitOfMeasure" />
	</xsd:simpleType>

	<xsd:simpleType name="LengthUnitOfMeasure" >
		<xsd:annotation>
			<xsd:documentation>Unit of measure code, whose type must be length</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="UnitOfMeasure" />
	</xsd:simpleType>

	<xsd:simpleType name="VolumeUnitOfMeasure"  >
		<xsd:annotation>
			<xsd:documentation>Unit of measure code, whose type must be volume</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="UnitOfMeasure" />
	</xsd:simpleType>

	<xsd:simpleType name="AreaUnitOfMeasure" >
		<xsd:annotation>
			<xsd:documentation>Unit of measure code, whose type must be area</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="UnitOfMeasure" />
	</xsd:simpleType>
	<!--Manufacturing Unit of Measure Types-->

	<xsd:complexType name="DimensionToStockRatioType" >
		<xsd:annotation>
			<xsd:documentation>One unit of measure = X stocking unit of measure</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="CalculatorUOM" type="UnitOfMeasure" minOccurs="1" maxOccurs="1"  />
			<xsd:element name="Quantity" type="xsd:double" minOccurs="1" maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="StockToDimensionRatioType" >
		<xsd:annotation>
			<xsd:documentation>One stocking unit of measure = X unit of measure</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Quantity" type="xsd:double" minOccurs="1" maxOccurs="1" />
			<xsd:element name="CalculatorUOM" type="UnitOfMeasure" minOccurs="1" maxOccurs="1"  />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="BarShape">
		<xsd:sequence>
			<xsd:choice minOccurs="0" maxOccurs="1">
				<xsd:element name="StockToDimensionRatio" type="StockToDimensionRatioType" />
				<xsd:element name="DimensionToStockRatio" type="DimensionToStockRatioType" />
			</xsd:choice>
			<xsd:element name="Measurements" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="MeasurementUnit" type="LengthUnitOfMeasure" minOccurs="0" maxOccurs="1" />
						<xsd:element name="Length" type="xsd:double" minOccurs="0" maxOccurs="1" />
						<xsd:element name="BarEnd" type="xsd:double" minOccurs="0" maxOccurs="1" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Weights" minOccurs="0" maxOccurs="1" >
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="WeightPer" type="LengthUnitOfMeasure" minOccurs="0" maxOccurs="1" />
						<xsd:element name="Weight" type="xsd:double" minOccurs="0" maxOccurs="1" />
						<xsd:element name="WeightUOM" type="WeightUnitOfMeasure" minOccurs="0" maxOccurs="1" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="CalculatedUnits" minOccurs="0" maxOccurs="1" >
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Weight" type="WeightUnitOfMeasure" minOccurs="0" maxOccurs="1" />
						<xsd:element name="Length" type="LengthUnitOfMeasure" minOccurs="0" maxOccurs="1" />
						<xsd:element name="WholeBars" type="xsd:boolean" default="false" minOccurs="0" maxOccurs="1" />
						<xsd:element name="WholePiecesPerBar" type="xsd:boolean" default="false" minOccurs="0" maxOccurs="1" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="SheetShape">
		<xsd:sequence>
			<xsd:choice minOccurs="0" maxOccurs="1">
				<xsd:element name="StockToDimensionRatio" type="StockToDimensionRatioType" />
				<xsd:element name="DimensionToStockRatio" type="DimensionToStockRatioType" />
			</xsd:choice>
			<xsd:element name="Measurements" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="MeasurementUnit" type="LengthUnitOfMeasure" minOccurs="0" maxOccurs="1" />
						<xsd:element name="Length" type="xsd:double" minOccurs="0" maxOccurs="1" />
						<xsd:element name="Width" type="xsd:double" minOccurs="0" maxOccurs="1" />
						<xsd:element name="Margin" type="xsd:double" minOccurs="0" maxOccurs="1" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Weights" minOccurs="0" maxOccurs="1" >
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="WeightPer" type="LengthUnitOfMeasure" minOccurs="0" maxOccurs="1" />
						<xsd:element name="Weight" type="xsd:double" minOccurs="0" maxOccurs="1" />
						<xsd:element name="WeightUOM" type="WeightUnitOfMeasure" minOccurs="0" maxOccurs="1" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="CalculatedUnits" minOccurs="0" maxOccurs="1" >
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Weight" type="WeightUnitOfMeasure" minOccurs="0" maxOccurs="1" />
						<xsd:element name="Area" type="AreaUnitOfMeasure" minOccurs="0" maxOccurs="1" />
						<xsd:element name="WholeSheets" type="xsd:boolean" default="false" minOccurs="0" maxOccurs="1" />
						<xsd:element name="WholePiecesPerSheet" type="xsd:boolean" default="false" minOccurs="0" maxOccurs="1" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CoilWireLengthShape">
		<xsd:sequence>
			<xsd:choice minOccurs="0" maxOccurs="1">
				<xsd:element name="StockToDimensionRatio" type="StockToDimensionRatioType" />
				<xsd:element name="DimensionToStockRatio" type="DimensionToStockRatioType" />
			</xsd:choice>
			<xsd:element name="Measurements" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="MeasurementUnit" type="LengthUnitOfMeasure" minOccurs="0" maxOccurs="1" />
						<xsd:element name="Width" type="xsd:double" minOccurs="0" maxOccurs="1" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Weights" minOccurs="0" maxOccurs="1" >
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="WeightPer" type="LengthUnitOfMeasure" minOccurs="0" maxOccurs="1" />
						<xsd:element name="Weight" type="xsd:double" minOccurs="0" maxOccurs="1" />
						<xsd:element name="WeightUOM" type="WeightUnitOfMeasure" minOccurs="0" maxOccurs="1" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="CalculatedUnits" minOccurs="0" maxOccurs="1" >
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Weight" type="WeightUnitOfMeasure" minOccurs="0" maxOccurs="1" />
						<xsd:element name="Length" type="LengthUnitOfMeasure" minOccurs="0" maxOccurs="1" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CoilWireWeightShape">
		<xsd:sequence>
			<xsd:choice minOccurs="0" maxOccurs="1">
				<xsd:element name="StockToDimensionRatio" type="StockToDimensionRatioType" />
				<xsd:element name="DimensionToStockRatio" type="DimensionToStockRatioType" />
			</xsd:choice>
			<xsd:element name="Measurements" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="MeasurementUnit" type="WeightUnitOfMeasure" minOccurs="0" maxOccurs="1" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="CalculatedUnits" minOccurs="0" maxOccurs="1" >
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Weight" type="WeightUnitOfMeasure" minOccurs="0" maxOccurs="1" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="VolumeShape">
		<xsd:sequence>
			<xsd:choice minOccurs="0" maxOccurs="1">
				<xsd:element name="StockToDimensionRatio" type="StockToDimensionRatioType" />
				<xsd:element name="DimensionToStockRatio" type="DimensionToStockRatioType" />
			</xsd:choice>
			<xsd:element name="Measurements" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="MeasurementUnit" type="LengthUnitOfMeasure" minOccurs="0" maxOccurs="1" />
						<xsd:element name="Length" type="xsd:double" minOccurs="0" maxOccurs="1" />
						<xsd:element name="Width" type="xsd:double" minOccurs="0" maxOccurs="1" />
						<xsd:element name="Depth" type="xsd:double" minOccurs="0" maxOccurs="1" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="CalculatedUnits" minOccurs="0" maxOccurs="1" >
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Volume" type="VolumeUnitOfMeasure" minOccurs="0" maxOccurs="1" />
						<xsd:element name="WholeEaches" type="xsd:boolean" default="false" minOccurs="0" maxOccurs="1" />
						<xsd:element name="WholePieces" type="xsd:boolean" default="false" minOccurs="0" maxOccurs="1" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!--Manufacturing Types (Private to schema) End-->

	<!-- Production Areas -->
	<xsd:element name="ProductionArea">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Description" minOccurs="1" maxOccurs="1"/>
				<xsd:element ref="Costcenter" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="Costunit" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="GLGeneralBurdenApplied" type="typeGLAccount" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="GLLaborApplied" type="typeGLAccount" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="GLLaborBurdenApplied" type="typeGLAccount" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="GLMachineBurdenApplied" type="typeGLAccount" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="GLGeneralBurdenCovered" type="typeGLAccount" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="GLLaborCovered" type="typeGLAccount" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="GLLaborBurdenCovered" type="typeGLAccount" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="GLMachineBurdenCovered" type="typeGLAccount" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="IsDefault" type="xsd:boolean"/>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
			<xsd:attribute ref="code" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ProductionAreas">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ProductionArea" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- Production Areas End -->

	<!-- Work centers -->
	<xsd:element name="WorkCenter">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Status" minOccurs="1" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>10=Active, 20=Inactive</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="20"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Type" minOccurs="1" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>10=Machine, 20=Person, 30=Cell</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="20"/>
							<xsd:enumeration value="30"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element ref="Description" minOccurs="1" maxOccurs="1"/>
				<xsd:element ref="Costcenter" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="Costunit" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="ProductionArea" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:attribute ref="ID" use="optional"/>
						<xsd:attribute ref="code" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="GeneralBurdenRate" type="xsd:float" default="0.00" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="LaborBurdenRate" type="xsd:float" default="0.00" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="MachineBurdenRate" type="xsd:float" default="0.00" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RunLaborRate" type="xsd:float" default="0.00" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="SellRate" type="xsd:float" default="0.00" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="SetupLaborRate" type="xsd:float" default="0.00" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="IsLaborBurdenPercent" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Capacity" type="xsd:int" default="1" minOccurs="0" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
			<xsd:attribute ref="code" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="WorkCenters">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="WorkCenter" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- Work centers End -->

	<!-- Operations -->
	<xsd:element name="Operation">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Description" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="Status" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation>10=Active, 20=Inactive</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="20"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
			<xsd:attribute ref="code" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Operations">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Operation" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- Operations End -->

	<!-- OperationResources -->
	<xsd:element name="OperationResource">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Operation" type="OperationReference"/>
				<xsd:choice minOccurs="1" maxOccurs="1">
					<xsd:element name="Supplier" type="SupplierResource"/>
					<xsd:element name="Workcenter" type="WorkcenterResource"/>
				</xsd:choice>
				<xsd:element name="Type" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>10=Workcenter, 20=Supplier</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="20"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="IsPrimary" type="xsd:boolean"/>
				<xsd:element name="IsActive" type="xsd:boolean"/>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="OperationResources">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="OperationResource" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- OperationResources End -->

	<!-- Manufacturing TimeEntry-->
	<xsd:element name="ManufacturingTimeTransaction">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Employee" minOccurs="1" maxOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Name" type="xsd:string" minOccurs="0" maxOccurs="1"/>
							<xsd:element name="EmployeeId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
							<xsd:element name="PersonId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
							<xsd:element name="HID" type="xsd:string" minOccurs="1" maxOccurs="1"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="EntryDate" type="xsd:date" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>YYYY-MM-DD</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Status" minOccurs="1" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>
							1=Draft,
							2=Rejected,
							10=Submitted,
							11=Failed on approval,
							14=Waiting to be approved,
							16=Waiting to be unapproved,
							19=Failed on unapproval,
							20=Final.

							Only status 1 (Draft) and 10 (Submitted) are available for import,
							but status 10 (Submitted) can only be used when the administration setting 'Approve: entries' is checked.
						</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:integer">
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="11"/>
							<xsd:enumeration value="14"/>
							<xsd:enumeration value="16"/>
							<xsd:enumeration value="19"/>
							<xsd:enumeration value="20"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="ShopOrder" minOccurs="1" maxOccurs="1">
					<xsd:complexType>
						<xsd:attribute ref="ID" use="optional"/>
						<xsd:attribute name="number" type="xsd:integer" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="RoutingStepPlan" minOccurs="1" maxOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Operation" minOccurs="1" maxOccurs="1">
								<xsd:complexType>
									<xsd:attribute ref="ID" use="optional"/>
									<xsd:attribute ref="code" use="required"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:choice>
								<xsd:element name="Workcenter" minOccurs="1" maxOccurs="1">
									<xsd:complexType>
										<xsd:attribute ref="ID" use="optional"/>
										<xsd:attribute ref="code" use="required"/>
									</xsd:complexType>
								</xsd:element>
								<xsd:element name="Supplier" minOccurs="1" maxOccurs="1">
									<xsd:complexType>
										<xsd:attribute ref="ID" use="optional"/>
										<xsd:attribute ref="code" use="required"/>
										<xsd:attribute name="name" type="xsd:string" use="optional"/>
									</xsd:complexType>
								</xsd:element>
							</xsd:choice>
						</xsd:sequence>
						<xsd:attribute ref="ID" use="optional"/>
						<xsd:attribute name="linenumber" type="xsd:integer" use="optional"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Workcenter" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:attribute ref="ID" use="optional"/>
						<xsd:attribute ref="code" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Activity" minOccurs="1" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>10=Setup, 20=Run</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="20"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Hours" type="xsd:float" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="LaborHours" type="xsd:float" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="Quantity" type="xsd:float" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="PercentComplete" type="typePercentage" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="IsOperationFinished" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ManufacturingTimeTransactions">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ManufacturingTimeTransaction" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- Manufacturing TimeEntry End -->

	<!-- Employee work center -->
	<xsd:element name="EmployeeWorkcenter">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Employee" minOccurs="1" maxOccurs="1">
					<xsd:complexType>
						<xsd:attribute name="EmployeeHID" type="xsd:integer" use="required"/>
						<xsd:attribute name="FullName" type="xsd:string" use="optional"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Workcenter" minOccurs="1" maxOccurs="1">
					<xsd:complexType>
						<xsd:attribute ref="ID" use="optional"/>
						<xsd:attribute ref="code" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="IsMain" type="xsd:boolean"/>
				<xsd:element name="Notes" type="xsd:string" minOccurs="0" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="EmployeeWorkcenters">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="EmployeeWorkcenter" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- Employee work center End -->

	<!-- Shop Order -->
	<xsd:element name="ShopOrder">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Description" minOccurs="1" maxOccurs="1"/>
				<xsd:element ref="YourRef" minOccurs="0"/>
				<xsd:element name="Status" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>10=Open, 20=In process, 30=Finished, 40=Complete.  Only used in export.  For import all orders are 10, Open.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="20"/>
							<xsd:enumeration value="30"/>
							<xsd:enumeration value="40"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="EntryDate" type="xsd:date" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>YYYY-MM-DD</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="PlannedStartDate" type="xsd:date" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>YYYY-MM-DD</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="PlannedDate" type="xsd:date" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>YYYY-MM-DD - PlannedDate = Planned end date of the shop order</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="ProductionLeadDays" type="xsd:integer" minOccurs="0" maxOccurs="1" />
				<xsd:element name="CadDrawingUrl" type="xsd:string" minOccurs="0" maxOccurs="1" />
				<xsd:element name="Item" minOccurs="1" maxOccurs="1">
					<xsd:complexType>
						<xsd:attribute ref="ID" use="optional"/>
						<xsd:attribute ref="code" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="PlannedQuantity" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="Costcenter" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="Costunit" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="IsInPlanning" type="xsd:boolean"/>
				<xsd:element name="IsReleased" type="xsd:boolean"/>
				<xsd:element name="IsOnHold" type="xsd:boolean"/>
				<xsd:element ref="Warehouse" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="MaterialPlans" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="ByProductPlans" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="RoutingPlans" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="Remarks" minOccurs="0" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
			<xsd:attribute ref="number" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ShopOrders">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ShopOrder" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- Shop Order End -->

	<!-- Shop Order Material Plan -->
	<xsd:element name="MaterialPlan">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="LineNumber" type="xsd:integer" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="Item" minOccurs="1" maxOccurs="1">
					<xsd:complexType>
						<xsd:attribute ref="ID" use="optional"/>
						<xsd:attribute ref="code" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element name="Status" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>5=Draft, 10=Open, 20=On order, 25=Partial, 30=Finished</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="5"/>
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="20"/>
							<xsd:enumeration value="25"/>
							<xsd:enumeration value="30"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="IsBackflushStep" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="CalculatorType" minOccurs="1" maxOccurs="1" />
				<xsd:element name="Quantity" type="xsd:float" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="WastePercentage" type="xsd:integer" minOccurs="0" maxOccurs="1" />
				<xsd:element name="DetailDrawing" type="xsd:string" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PlannedDate" type="xsd:date" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>YYYY-MM-DD</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="PlannedPrice" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Rate" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Currency" type="xsd:string" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="Costcenter" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="Costunit" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="CertificationRequired" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RoutingStep" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="typeIntegerEmptyString">
								<xsd:attribute name="ID" type="xsd:string" use="optional" />
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="MaterialPlans">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="MaterialPlan" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- Shop Order Material Plan End -->

	<!-- Shop Order ByProduct Plan -->
	<xsd:element name="ByProductPlan">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="LineNumber" type="xsd:integer" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="Item" minOccurs="1" maxOccurs="1">
					<xsd:complexType>
						<xsd:attribute ref="ID" use="optional"/>
						<xsd:attribute ref="code" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element name="Status" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>5=Draft, 10=Open, 20=On order, 25=Partial, 30=Finished</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="5"/>
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="20"/>
							<xsd:enumeration value="25"/>
							<xsd:enumeration value="30"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="IsBackflushStep" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="CalculatorType" minOccurs="1" maxOccurs="1" />
				<xsd:element name="Quantity" type="xsd:float" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="DetailDrawing" type="xsd:string" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PlannedDate" type="xsd:date" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>YYYY-MM-DD</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="PlannedPrice" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Rate" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Currency" type="xsd:string" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="Costcenter" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="Costunit" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="CertificationRequired" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RoutingStep" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="typeIntegerEmptyString">
								<xsd:attribute name="ID" type="xsd:string" use="optional" />
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ByProductPlans">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ByProductPlan" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- Shop Order ByProduct Plan End -->

	<!-- Shop Order Routing Plan -->
	<xsd:element name="RoutingPlan">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="LineNumber" type="xsd:integer" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="Operation" type="OperationReference" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="OperationDescription" type="xsd:string" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="Description" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Workcenter" type="WorkcenterReference" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="OperationResource" type="OperationResourceReference" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="Item" type="ItemReference" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Status" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>10=Open, 20=In process, 30=Finished</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="20"/>
							<xsd:enumeration value="30"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="RoutingStepType" minOccurs="1" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>10=work center, 20=Supplier</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="20"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Supplier" type="SupplierReference" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Setup" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="SetupUnit" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>hh=Hour, mi=Minute, ss=Second</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="hh"/>
							<xsd:enumeration value="mi"/>
							<xsd:enumeration value="ss"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Run" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RunMethod" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>10=Fixed hours, 20=Fixed minutes, 25=Fixed days, 30=Hours/Item, 40=Minutes/Item, 50=Seconds/Item, 60=Items/Hour, 70=Items/Minute</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="20"/>
							<xsd:enumeration value="25"/>
							<xsd:enumeration value="30"/>
							<xsd:enumeration value="40"/>
							<xsd:enumeration value="50"/>
							<xsd:enumeration value="60"/>
							<xsd:enumeration value="70"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="EfficiencyPercentage" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="AttendedPercentage" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="SetupLaborBurdenRate" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="SetupLaborRate" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PlannedRunHours" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PlannedSetupHours" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PlannedTotalHours" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RunLaborBurdenRate" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RunLaborRate" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="MachineBurdenRate" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="GeneralBurdenRate" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Currency" type="xsd:string" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PurchaseUnit" type="xsd:string" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PurchaseUnitFactor" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="IsPlannedMinimumChargeApplied" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="SubcontractedLeadDays" type="xsd:int" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="IsReworkStep" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="IsCommunicationStep" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="IsBackflushStep" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="SetupLabor" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="SetupLaborBurden" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RunLabor" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RunLaborBurden" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="MachineBurden" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="GeneralBurden" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PurchaseUnitQuantity" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PurchaseUnitPriceFC" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PurchaseUnitPriceDC" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="MinimumChargeFC" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="MinimumChargeDC" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RateFC" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="CostPerItem" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="FactorType" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>1=Purchase Unit Over Order Unit, 2=Order Unit Over Purchase Unit</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="RemainingEstimatedSetupHours" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RemainingEstimatedRunHours" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RemainingCalculatedRunHours" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RemainingActualSetupHours" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RemainingActualRunHours" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PlannedStartDate" type="xsd:dateTime" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>YYYY-MM-DDThh:mm:ss</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="PlannedEndDate" type="xsd:dateTime" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>YYYY-MM-DDThh:mm:ss</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="DueDate" type="xsd:date" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>YYYY-MM-DD</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="RoutingPlans">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="RoutingPlan" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- Shop Order Routing Plan End -->
	<!-- Serial Batch numbers Start -->
	<xsd:simpleType name="SerialBatchNumber">
		<xsd:annotation>
			<xsd:documentation>Serial number or Batch number code</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="60" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:element name="SerialNumber">
		<xsd:annotation>
			<xsd:documentation>Serial number</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Parts" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attribute name="SerialNumber" type="SerialBatchNumber" use="required"/>
			<xsd:attribute name="StartDate" use="optional" type="xsd:date"/>
			<xsd:attribute name="EndDate" use="optional" type="xsd:date"/>
			<xsd:attribute name="Notes" use="optional" type="xsd:string"/>
			<xsd:attribute name="Blocked" use="optional" type="xsd:boolean"/>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="SerialNumbers">
		<xsd:annotation>
			<xsd:documentation>List of serial numbers</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="SerialNumber" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="BatchNumber">
		<xsd:annotation>
			<xsd:documentation>Batch number</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Parts" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attribute name="BatchNumber" type="SerialBatchNumber" use="required"/>
			<xsd:attribute name="ExpiryDate" use="optional" type="xsd:date"/>
			<xsd:attribute name="Notes" use="optional" type="xsd:string"/>
			<xsd:attribute name="Blocked" use="optional" type="xsd:boolean"/>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="BatchNumberLine">
		<xsd:annotation>
			<xsd:documentation>Batch number line</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="BatchNumber" minOccurs="1" maxOccurs="1" />
			</xsd:sequence>
			<xsd:attribute name="Quantity" use="required" type="xsd:float"/>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="BatchNumbers">
		<xsd:annotation>
			<xsd:documentation>List of batch numbers</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="BatchNumberLine" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<!-- Serial Batch numbers End -->
	<!-- StockTransactionLocations Start -->
	<xsd:element name="StockTransactionLocations">
		<xsd:annotation>
			<xsd:documentation>List stock transaction locations</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="StockTransactionLocation" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- StockTransactionLocations End -->
	<!-- StockTransactionLocation Start -->
	<xsd:element name="StockTransactionLocation">
		<xsd:annotation>
			<xsd:documentation>stock transaction locations</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="StorageLocation" minOccurs="1" maxOccurs="1"></xsd:element>
				<xsd:choice minOccurs="0" maxOccurs="1">
					<xsd:element ref="SerialNumbers" />
					<xsd:element ref="BatchNumbers" />
				</xsd:choice>
			</xsd:sequence>
			<xsd:attribute name="Quantity" type="xsd:float" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<!-- StockTransactionLocation End -->
	<!-- Material Issues -->
	<xsd:element name="MaterialIssue">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="ShopOrderNumber" type="xsd:integer" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="MaterialPlanLineNumber" type="xsd:integer" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="ItemCode" type="xsd:string" minOccurs="1" maxOccurs="1"/>
				<xsd:choice minOccurs="1" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>
							SplitIssueLine=Only used in Manufacturing Premium Package when Warehouse uses Storage locations
						</xsd:documentation>
					</xsd:annotation>
					<xsd:element name="Quantity" type="xsd:float"/>
					<xsd:element ref="SplitIssueLine" minOccurs="1" maxOccurs="unbounded"/>
				</xsd:choice>
				<xsd:element ref="Warehouse" minOccurs="0"/>
				<xsd:choice minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>
							Only used in conjuction with Manufacturing Basic and Premium
							If Manufacturinng Premium then Serial/Batch is included in SplitIssueLine
						</xsd:documentation>
					</xsd:annotation>
					<xsd:element ref="SerialNumbers" />
					<xsd:element ref="BatchNumbers" />
				</xsd:choice>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="MaterialIssues">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="MaterialIssue" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SplitIssueLine">
		<xsd:complexType>
			<xsd:sequence maxOccurs="1">
				<xsd:element ref="StockTransactionLocations" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute name="Quantity" use="required" type="xsd:float"/>
		</xsd:complexType>
	</xsd:element>
	<!-- Material Issues End -->

	<!-- Material Purchase Orders -->
	<xsd:element name="MaterialPurchaseOrder">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="AssignSupplierVia" minOccurs="1" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>1=Main supplier, 2=Lowest purchase price, 3=Shortest lead time</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
							<xsd:enumeration value="3"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="CombineLikeRequirements" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Requirements">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Requirement" maxOccurs="unbounded"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Requirement">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="ShopOrderNumber" type="xsd:integer" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="MaterialPlanLineNumber" type="xsd:integer" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="ItemCode" type="xsd:string" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="Quantity" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Price" type="xsd:float" minOccurs="0" maxOccurs="1"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="MaterialPurchaseOrders">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="MaterialPurchaseOrder" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- Material Purchase Orders End -->

	<!-- Shop Order Stock Receipts-->
	<xsd:element name="ShopOrderStockReceipt">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="ShopOrderNumber" type="xsd:integer" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="ItemCode" type="xsd:string" minOccurs="0" maxOccurs="1"/>
				<xsd:choice minOccurs="1" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>
							SplitStockReceipt=Used in Manufacturing Premium Package when warehouse uses Storage locations
							-- Only available for "Put in stock" receipts and "By Product" receipts
							Quantity=Used in Manufacturing Basic and Advanced Packages
						</xsd:documentation>
					</xsd:annotation>
					<xsd:element name="Quantity" type="xsd:float" minOccurs="1" maxOccurs="1"/>
					<xsd:element ref="SplitShopOrderStock" minOccurs="1" maxOccurs="unbounded"/>
					<xsd:element ref="SplitByProduct" minOccurs="1" maxOccurs="unbounded"/>
				</xsd:choice>
				<xsd:element name="Disposition" minOccurs="1" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>2=Stage for delivery, 3=Put in stock, 4=Issue to parent, 5=By product</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="2"/>
							<xsd:enumeration value="3"/>
							<xsd:enumeration value="4"/>
							<xsd:enumeration value="5"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="TransactionDate" type="xsd:date" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>YYYY-MM-DD</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="Warehouse" minOccurs="0"/>
				<xsd:element name="ByProductPlan" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:attribute  name="Id" type="xsd:string" use="required"/>
						<xsd:attribute  name="LineNumber" type="xsd:int" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:choice minOccurs="0" maxOccurs="1">
					<xsd:element ref="SerialNumbers" />
					<xsd:element ref="BatchNumbers" />
				</xsd:choice>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ShopOrderStockReceipts">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ShopOrderStockReceipt" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="SplitByProduct">
		<xsd:complexType>
			<xsd:sequence maxOccurs="1">
				<xsd:element ref="StockTransactionLocations" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute name="Id" type="xsd:string" use="required"/>
			<xsd:attribute name="LineNumber" type="xsd:int" use="required"/>
			<xsd:attribute name="Quantity" use="required" type="xsd:float"/>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="SplitShopOrderStock">
		<xsd:complexType>
			<xsd:sequence maxOccurs="1">
				<xsd:element ref="StockTransactionLocations" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute name="Quantity" use="required" type="xsd:float"/>
		</xsd:complexType>
	</xsd:element>
	<!-- Shop Order Stock Receipts End-->

	<!-- Subcontract Purchase Orders  -->
	<xsd:element name="SubcontractPurchaseOrder">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="AssignSupplierVia" minOccurs="1" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>1=Routing Step, 2=Shortest lead time</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Requirements">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Requirement" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="ShopOrderNumber" type="xsd:integer" minOccurs="1" maxOccurs="1"/>
										<xsd:element name="RoutingStepLineNumber" type="xsd:integer" minOccurs="1" maxOccurs="1"/>
										<xsd:element name="ItemCode" type="xsd:string" minOccurs="1" maxOccurs="1"/>
										<xsd:element name="Quantity" type="xsd:float" minOccurs="0" maxOccurs="1"/>
										<xsd:element name="Price" type="xsd:float" minOccurs="0" maxOccurs="1"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SubcontractPurchaseOrders">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="SubcontractPurchaseOrder" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- Subcontract Purchase Orders End -->
	<!-- Manufacturing Bill Of Material Start -->
	<xsd:element name="ManufacturedBillofMaterial">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="VersionNumber" type="xsd:integer" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="VersionDate" type="xsd:date" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>YYYY-MM-DD</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="Description" minOccurs="1" maxOccurs="1"/>
				<xsd:element ref="Item"/>
				<xsd:element name="Status" minOccurs="1" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>10=Engineering change pending, 20=Engineering change approved, 30=Active, 40=Historic</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="20"/>
							<xsd:enumeration value="30"/>
							<xsd:enumeration value="40"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="IsDefault" type="xsd:integer" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>IsDefault only applicable to Active status otherwise it can be ignore during import</xsd:documentation>
					</xsd:annotation>
				</xsd:element >
				<xsd:element name="CalculatedCostPrice" type="xsd:float" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>CalculatedCostPrice is obsolete. Always being calculated.</xsd:documentation>
					</xsd:annotation>
				</xsd:element >
				<xsd:element name="Remarks" type="xsd:string" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="BatchQuantity" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="ManufacturedBillofMaterialItemDetails" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="ManufacturedBillofMaterialRoutingDetails" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="ManufacturedBillofMaterialByProductDetails" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="LeadTime" type="xsd:integer" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>This property is the production lead days of the shop order.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="OrderLeadDays" type="xsd:integer" minOccurs="0" maxOccurs="1" />
				<xsd:element name="CADDrawingURL" type="xsd:string" minOccurs="0" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="ManufacturedBillofMaterialItemDetail">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="LineNumber" type="xsd:integer" minOccurs="1" maxOccurs="1"/>
				<xsd:element ref="Item"/>
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element name="CalculatedCostPrice" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Quantity" type="xsd:float" minOccurs="1" maxOccurs="1"/>
				<xsd:element ref="CalculatorType" minOccurs="1" maxOccurs="1" />
				<xsd:element name="WastePercentage" type="xsd:integer" minOccurs="0" maxOccurs="1" />
				<xsd:element name="Notes" type="xsd:string" minOccurs="0" maxOccurs="1" />
				<xsd:element name="DetailDrawing" type="xsd:string" minOccurs="0" maxOccurs="1" />
				<xsd:element name="Backflush" type="xsd:integer" minOccurs="0" maxOccurs="1" />
				<xsd:element name="CostCenter" type="xsd:string" minOccurs="0" maxOccurs="1" />
				<xsd:element name="CostUnit" type="xsd:string" minOccurs="0" maxOccurs="1" />
				<xsd:element name="NetWeight" minOccurs="0">
					<xsd:complexType>
						<xsd:annotation>
							<xsd:documentation>NetWeight property is only used for exports. Imports will ignore NetWeight because it is calculated</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleContent>
							<xsd:extension base="xsd:float">
								<xsd:attribute name="NetWeightUnit" type="xsd:string"/>
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="RoutingStep" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="typeIntegerEmptyString">
								<xsd:attribute name="ID" type="xsd:string" use="optional" />
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="ManufacturedBillofMaterialItemDetails">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ManufacturedBillofMaterialItemDetail" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="ManufacturedBillofMaterialRoutingDetail">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="LineNumber" type="xsd:integer" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="Operation" type="OperationReference" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="OperationDescription" type="xsd:string" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Workcenter" type="WorkcenterReference" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Supplier" type="SupplierReference" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="OperationResource" type="OperationResourceReference" minOccurs="1" maxOccurs="1"/>
				<xsd:element ref="Item" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RoutingStepType" minOccurs="1" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>10=work center, 20=Supplier</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="20"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Setup" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="SetupUnit" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>hh=Hour, mi=Minute, ss=Second</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="hh"/>
							<xsd:enumeration value="mi"/>
							<xsd:enumeration value="ss"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Run" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RunMethod" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>10=Fixed hours, 20=Fixed minutes, 25=Fixed days, 30=Hours/Item, 40=Minutes/Item, 50=Seconds/Item, 60=Items/Hour, 70=Items/Minute</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="20"/>
							<xsd:enumeration value="25"/>
							<xsd:enumeration value="30"/>
							<xsd:enumeration value="40"/>
							<xsd:enumeration value="50"/>
							<xsd:enumeration value="60"/>
							<xsd:enumeration value="70"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="EfficiencyPercentage" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="AttendedPercentage" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="SetupLaborBurdenRate" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="SetupLaborRate" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PlannedRunHours" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PlannedSetupHours" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PlannedTotalHours" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RunLaborBurdenRate" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RunLaborRate" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="MachineBurdenRate" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="GeneralBurdenRate" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Currency" type="xsd:string" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PurchaseUnit" type="xsd:string" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PurchaseUnitFactor" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="SubcontractedLeadDays" type="xsd:int" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="IsBackflushStep" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="SetupLabor" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="SetupLaborBurden" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RunLabor" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RunLaborBurden" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="MachineBurden" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="GeneralBurden" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PurchaseUnitQuantity" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PurchaseUnitPriceFC" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="PurchaseUnitPriceDC" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="MinimumChargeFC" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="MinimumChargeDC" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="RateFC" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="CostPerItem" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="TotalCostDC" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="FactorType" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>1=Purchase Unit Over Order Unit, 2=Order Unit Over Purchase Unit</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Notes" type="xsd:string" minOccurs="0" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ManufacturedBillofMaterialRoutingDetails">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ManufacturedBillofMaterialRoutingDetail" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="ManufacturedBillofMaterialByProductDetail">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="LineNumber" type="xsd:integer" minOccurs="1" maxOccurs="1"/>
				<xsd:element ref="Item"/>
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element name="CalculatedCostPrice" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Quantity" type="xsd:float" minOccurs="1" maxOccurs="1"/>
				<xsd:element ref="CalculatorType" minOccurs="1" maxOccurs="1" />
				<xsd:element name="Notes" type="xsd:string" minOccurs="0" maxOccurs="1" />
				<xsd:element name="DetailDrawing" type="xsd:string" minOccurs="0" maxOccurs="1" />
				<xsd:element name="Backflush" type="xsd:integer" minOccurs="0" maxOccurs="1" />
				<xsd:element name="CostCenter" type="xsd:string" minOccurs="0" maxOccurs="1" />
				<xsd:element name="CostUnit" type="xsd:string" minOccurs="0" maxOccurs="1" />
				<xsd:element name="RoutingStep" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="typeIntegerEmptyString">
								<xsd:attribute name="ID" type="xsd:string" use="optional" />
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="ManufacturedBillofMaterialByProductDetails">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ManufacturedBillofMaterialByProductDetail" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="ManufacturedBillofMaterials">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ManufacturedBillofMaterial" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- Manufacturing Bill Of Material End -->

	<!-- Bill Of Material Start -->
	<xsd:element name="BillOfMaterial">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Item" minOccurs="1" maxOccurs="1"/>
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element name="CostPrice" type="xsd:float" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="BatchQuantity" type="xsd:float" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="AssembledLeadDays" type="xsd:int" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="AssembleAtDelivery" type="xsd:boolean" minOccurs="0"/>
				<xsd:element ref="BillOfMaterialItemDetails" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="BillOfMaterialItemDetail">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="LineNumber" type="xsd:integer" minOccurs="1" maxOccurs="1"/>
				<xsd:element ref="Item" minOccurs="1" maxOccurs="1"/>
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element name="QuantityPerBatch" type="xsd:float" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="BillOfMaterialItemDetails">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="BillOfMaterialItemDetail" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="BillOfMaterials">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="BillOfMaterial" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- Bill Of Material End -->

	<!-- FeatureSet type Start -->
	<xsd:complexType name="typeFeatureSets">
		<xsd:sequence>
			<xsd:element name="FeatureSet" maxOccurs="unbounded" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- FeatureSet type End -->
	<!-- Legislation type Start -->
	<xsd:simpleType name="typeLegislation">
		<xsd:list itemType="typeLegislationEnum"/>
	</xsd:simpleType>
	<xsd:simpleType name="typeLegislationEnum">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Belgium"/>
			<xsd:enumeration value="France"/>
			<xsd:enumeration value="Germany"/>
			<xsd:enumeration value="Luxembourg"/>
			<xsd:enumeration value="Netherlands"/>
			<xsd:enumeration value="NetherlandsAntilles"/>
			<xsd:enumeration value="Spain"/>
			<xsd:enumeration value="Turkey"/>
			<xsd:enumeration value="UnitedKingdom"/>
			<xsd:enumeration value="UnitedStates"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Legislation type Start -->
	<!-- Simple types Start -->
	<xsd:simpleType name="typeString255">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="typeString128">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="128"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="typeString60">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="60"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="typeString35">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="35"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="typeIntegerEmptyString">
		<xsd:union>
			<xsd:simpleType>
				<xsd:restriction base="xsd:integer"/>
			</xsd:simpleType>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:length value="0"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:union>
	</xsd:simpleType>
	<!-- Simple types End -->
	<!-- Direct Debit Mandates type Start -->
	<xsd:complexType name="typeDDMandates" >
		<xsd:sequence>
			<xsd:element name="DDMandate" type="typeDDMandate" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="typeDDMandate" >
		<xsd:sequence minOccurs="0">
			<xsd:element name="Description" type="typeDescription" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="Account" type="typeAccount" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="BankAccount" type="typeBankAccount" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="Type" type="typeDDMandateType" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="PaymentType" type="typeDDMandatePaymentType" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="SignatureDate" type="xsd:date" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="FirstSend" type="xsd:boolean" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="CancellationDate" type="xsd:date" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="Attachment" type="typeAttachment" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
		<xsd:attribute name="ID" type="xsd:string" use="optional"/>
		<xsd:attribute name="reference" type="typeString35" use="required"/>
		<xsd:attribute ref="default" use="optional"/>
	</xsd:complexType>
	<xsd:simpleType name="typeDDMandateType">
		<xsd:list itemType="typeDDMandateTypeEnum"/>
	</xsd:simpleType>
	<xsd:simpleType name="typeDDMandateTypeEnum">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Core"/>
			<xsd:enumeration value="B2B"/>
			<xsd:enumeration value="Bottomline"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="typeDDMandatePaymentType">
		<xsd:list itemType="typeDDMandatePaymentTypeEnum"/>
	</xsd:simpleType>
	<xsd:simpleType name="typeDDMandatePaymentTypeEnum">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="OneOff"/>
			<xsd:enumeration value="Recurrent"/>
			<xsd:enumeration value="AdHoc"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Direct Debit Mandates type End -->
	<!-- Mailboxes Start -->
	<xsd:complexType name="typeDigitalPostBoxes">
		<xsd:sequence>
			<xsd:element name="DigitalPostBox" type="typeDigitalPostBox" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="typeDigitalPostBox">
		<xsd:sequence>
			<xsd:element name="Description" type="typeDescription" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
		<xsd:attribute name="mailbox" type="typeString128" use="required"/>
	</xsd:complexType>
	<!-- Mailboxes End -->
	<xsd:element name="Budgets">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Budget" maxOccurs="unbounded" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence minOccurs="1">
							<xsd:element name="Scenario" maxOccurs="1">
								<xsd:complexType>
									<xsd:attribute  ref="code" use="required"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="FinancialYear" type="xsd:integer" maxOccurs="1"/>
							<xsd:element name="BudgetLines" maxOccurs="1">
								<xsd:complexType>
									<xsd:sequence minOccurs="1">
										<xsd:element name="BudgetLine" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:sequence minOccurs="1">
													<xsd:element ref="GLAccount" />
													<xsd:element ref="Costcenter" minOccurs="0"/>
													<xsd:element ref="Costunit" minOccurs="0"/>
													<xsd:element ref="Item" minOccurs="0"/>
													<xsd:element name="Periods" maxOccurs="1" >
														<xsd:complexType>
															<xsd:sequence minOccurs="1">
																<xsd:element name="Period" maxOccurs="unbounded">
																	<xsd:complexType>
																		<xsd:attribute  name="value" type="xsd:int" use="required"/>
																		<xsd:attribute  name="amount" type="xsd:float" use="required"/>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- BudgetScenarios Start -->
	<xsd:element name="BudgetScenarios">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="BudgetScenario" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- BudgetScenarios End -->
	<!-- BudgetScenario Start -->
	<xsd:element name="BudgetScenario">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Description" minOccurs="1"/>
				<xsd:element name="FromFinancialYear" minOccurs="0"/>
				<xsd:element name="ToFinancialYear" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute ref="code"/>
		</xsd:complexType>
	</xsd:element>
	<!-- BudgetScenario End -->
	<!-- TaxSchedule Start -->
	<xsd:element name="TaxSchedule">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Description" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="Type" type="xsd:integer" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="IsBlocked" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Notes" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="TaxComponents" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
			<xsd:attribute ref="code" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<!-- TaxSchedules End -->
	<!-- TaxSchedules Start -->
	<xsd:element name="TaxSchedules">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="TaxSchedule" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- TaxSchedules End -->
	<!-- TaxComponent Start -->
	<xsd:element name="TaxComponent">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Description" minOccurs="1" maxOccurs="1"/>
				<xsd:element ref="GLAccount" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="Account" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="Notes" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="TaxComponentRates" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
			<xsd:attribute ref="code" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<!-- TaxComponent End -->
	<!-- TaxComponents Start -->
	<xsd:element name="TaxComponents">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="TaxComponent" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- TaxComponents End -->
	<!-- TaxComponentRate Start -->
	<xsd:element name="TaxComponentRate">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Rate" type="xsd:float" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="LineNumber" type="xsd:integer" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="StartDate" type="xsd:date" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>YYYY-MM-DD</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="EndDate" type="xsd:date" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
