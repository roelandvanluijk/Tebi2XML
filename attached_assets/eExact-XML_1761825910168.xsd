				<xsd:element ref="Students" minOccurs="0"/>
				<xsd:element ref="Documents" minOccurs="0"/>
				<xsd:element ref="GLTransactions" minOccurs="0"/>
				<xsd:element ref="Indicators" minOccurs="0"/>
				<xsd:element ref="Invoices" minOccurs="0"/>
				<xsd:element ref="PurchaseOrders" minOccurs="0"/>
				<xsd:element ref="SalesOrders" minOccurs="0"/>
				<xsd:element ref="GLAccounts" minOccurs="0"/>
				<xsd:element ref="GLAccountCategories" minOccurs="0"/>
				<xsd:element ref="GLAccountClassifications" minOccurs="0"/>
				<xsd:element ref="VATs" minOccurs="0"/>
				<xsd:element ref="Journals" minOccurs="0"/>
				<xsd:element ref="Costcenters" minOccurs="0"/>
				<xsd:element ref="Costunits" minOccurs="0"/>
				<xsd:element ref="JobTitles" minOccurs="0"/>
				<xsd:element ref="JobGroups" minOccurs="0"/>
				<xsd:element ref="Departments" minOccurs="0"/>
				<xsd:element ref="Employees" minOccurs="0"/>
				<xsd:element ref="Employments" minOccurs="0"/>
				<xsd:element ref="Agencies" minOccurs="0"/>
				<xsd:element ref="PayrollYears" minOccurs="0"/>
				<xsd:element ref="PayrollGLLinks" minOccurs="0"/>
				<xsd:element ref="PayrollComponents" minOccurs="0"/>
				<xsd:element ref="PayrollComponentGroups" minOccurs="0"/>
				<xsd:element ref="PayrollEntries" minOccurs="0"/>
				<xsd:element ref="PaymentConditions" minOccurs="0"/>
				<xsd:element ref="FinYears" minOccurs="0"/>
				<xsd:element name="Settings" type="typeSettings" minOccurs="0"/>
				<xsd:element ref="Titles" minOccurs="0"/>
				<xsd:element ref="Assets" minOccurs="0"/>
				<xsd:element ref="AssetGroups" minOccurs="0"/>
				<xsd:element ref="DepreciationMethods" minOccurs="0"/>
				<xsd:element ref="MatchSets" minOccurs="0"/>
				<xsd:element ref="ActivitySectors" minOccurs="0"/>
				<xsd:element ref="SBICode" minOccurs="0"/>
				<xsd:element ref="CompanySizes" minOccurs="0"/>
				<xsd:element ref="BusinessTypes" minOccurs="0"/>
				<xsd:element ref="LeadSources" minOccurs="0"/>
				<xsd:element ref="SalesTypes" minOccurs="0"/>
				<xsd:element ref="ReasonCodes" minOccurs="0"/>
				<xsd:element ref="OpportunityStages" minOccurs="0"/>
				<xsd:element ref="Opportunities" minOccurs="0"/>
				<xsd:element ref="Quotations" minOccurs="0"/>
				<xsd:element ref="LeadStages" minOccurs="0"/>
				<xsd:element ref="LeadRatings" minOccurs="0"/>
				<xsd:element ref="Leads" minOccurs="0"/>
				<xsd:element ref="Users" minOccurs="0"/>
				<xsd:element ref="UserAdministrations" minOccurs="0"/>
				<xsd:element ref="ExchangeRates" minOccurs="0"/>
				<xsd:element ref="TimeTransactions" minOccurs="0"/>
				<xsd:element ref="CostTransactions" minOccurs="0"/>
				<xsd:element ref="SubscriptionTypes" minOccurs="0"/>
				<xsd:element ref="Subscriptions" minOccurs="0"/>
				<xsd:element ref="HRMSubscriptions" minOccurs="0"/>
				<xsd:element ref="Projects" minOccurs="0"/>
				<xsd:element ref="ManufacturingProjects" minOccurs="0"/>
				<xsd:element ref="WorkBreakdownStructures" minOccurs="0"/>
				<xsd:element ref="ProjectPlanning" minOccurs="0"/>
				<xsd:element ref="WBSTemplates" minOccurs="0"/>
				<xsd:element ref="StockPositions" minOccurs="0"/>
				<xsd:element ref="StockCounts" minOccurs="0"/>
				<xsd:element ref="Deliveries" minOccurs="0"/>
				<xsd:element ref="Receipts" minOccurs="0"/>
				<xsd:element ref="AllocationRules" minOccurs="0"/>
				<xsd:element ref="CreditManagementScenarios" minOccurs="0"/>
				<xsd:element ref="CreditManagementStatuses" minOccurs="0"/>
				<xsd:element ref="TrackingNumbers" minOccurs="0"/>
				<xsd:element ref="AdministrationTypes" minOccurs="0"/>
				<xsd:element ref="UserTypes" minOccurs="0"/>
				<xsd:element ref="ShippingMethods" minOccurs="0"/>
				<xsd:element ref="EntryTemplates" minOccurs="0"/>
				<xsd:element ref="EntryTemplateLinks" minOccurs="0"/>
				<xsd:element ref="EntryTemplateTransactions" minOccurs="0"/>
				<xsd:element ref="Operations" minOccurs="0"/>
				<xsd:element ref="WorkCenters" minOccurs="0"/>
				<xsd:element ref="ProductionAreas" minOccurs="0"/>
				<xsd:element ref="OperationResources" minOccurs="0"/>
				<xsd:element ref="ShopOrders" minOccurs="0"/>
				<xsd:element ref="ManufacturingTimeTransactions" minOccurs="0"/>
				<xsd:element ref="EmployeeWorkcenters" minOccurs="0"/>
				<xsd:element ref="MaterialIssues" minOccurs="0"/>
				<xsd:element ref="ShopOrderStockReceipts" minOccurs="0" />
				<xsd:element ref="MaterialPurchaseOrders" minOccurs="0"/>
				<xsd:element ref="SubcontractPurchaseOrders" minOccurs="0"/>
				<xsd:element ref="ManufacturedBillofMaterials" minOccurs="0"/>
				<xsd:element ref="UserProfiles" minOccurs="0"/>
				<xsd:element name="DDMandates" type="typeDDMandates" minOccurs="0" />
				<xsd:element ref="Warehouses" minOccurs="0"/>
				<xsd:element ref="BillOfMaterials" minOccurs="0"/>
				<xsd:element ref="AssemblyOrders" minOccurs="0"/>
				<xsd:element ref="WarehouseTransfers" minOccurs="0"/>
				<xsd:element ref="Budgets" minOccurs="0"/>
				<xsd:element ref="BudgetScenarios" minOccurs="0"/>
				<xsd:element ref="FinishedAssemblies" minOccurs="0"/>
				<xsd:element ref="WebShopSalesOrders" minOccurs="0"/>
				<xsd:element ref="NationalHolidays" minOccurs="0"/>
				<xsd:element ref="TaxSchedules" minOccurs="0" />
				<xsd:element ref="TaxComponents" minOccurs="0"/>
				<xsd:element ref="SalesForecasts" minOccurs="0"/>
				<xsd:element ref="Deposits" minOccurs="0"/>
				<xsd:element ref="ExtraFieldDefinitions" minOccurs="0"/>
				<xsd:element ref="LeadPurposes" minOccurs="0"/>
				<xsd:element ref="WageScales" minOccurs="0"/>
				<xsd:element ref="EmployeeItems" minOccurs="0"/>
				<xsd:element ref="ProjectClassifications" minOccurs="0"/>
				<xsd:element ref="ReasonsLogistics" minOccurs="0"/>
				<xsd:element ref="PeriodClosingTemplates" minOccurs="0"/>
				<xsd:element ref="ClientAcceptanceTemplates" minOccurs="0"/>
				<!-- 
			The following elements will be added automatically during export and aren't needed for import.
		-->
				<xsd:element ref="Topics" minOccurs="0"/>
				<xsd:element ref="Messages" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="schemaVersion" type="xsd:string" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<!--Topics & Messages-->
	<xsd:element name="Topics">
		<xsd:annotation>
			<xsd:documentation>Topic and timestamp information</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Topic" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Topic">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="Data" minOccurs="0" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:attribute name="key" type="xsd:string"/>
						<xsd:attribute name="keyAlt" type="xsd:string"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Parameters" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:sequence minOccurs="0">
							<xsd:element name="Export" minOccurs="0" maxOccurs="1">
								<xsd:complexType>
									<xsd:sequence minOccurs="0">
										<xsd:element ref="Parameter" minOccurs="0" maxOccurs="unbounded"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Import" minOccurs="0" maxOccurs="1">
								<xsd:complexType>
									<xsd:sequence minOccurs="0">
										<xsd:element ref="Parameter" minOccurs="0" maxOccurs="unbounded"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute ref="code" use="optional"/>
			<xsd:attribute name="node" type="xsd:string" use="optional"/>
			<xsd:attribute name="ts_d" type="xsd:string" use="optional"/>
			<xsd:attribute name="ts_u" type="xsd:string" use="optional"/>
			<xsd:attribute name="count" type="xsd:integer" use="optional"/>
			<xsd:attribute name="pagesize" type="xsd:integer" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<!--Topic parameters-->
	<xsd:element name="Parameter">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="ParameterValues" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="ParameterValue" minOccurs="0" maxOccurs="unbounded"/>
						</xsd:sequence>
						<xsd:attribute name="multiselect" use="optional">
							<xsd:simpleType>
								<xsd:restriction base="xsd:integer">
									<xsd:enumeration value="0"/>
									<xsd:enumeration value="1"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="name" type="xsd:string" use="required"/>
			<xsd:attribute name="description" type="xsd:string" use="optional"/>
			<xsd:attribute name="type" type="xsd:string" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ParameterValue">
		<xsd:complexType>
			<xsd:attribute name="value" type="xsd:string" use="required"/>
			<xsd:attribute name="description" type="xsd:string" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<!--Root Elements-->
	<xsd:element name="Message">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Topic" minOccurs="0"/>
				<xsd:element name="Date" type="xsd:dateTime">
					<xsd:annotation>
						<xsd:documentation>YYYY-MM-DDThh:mm:ss</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="Description"/>
				<xsd:element name="InnerXML" type="xsd:string" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="type" use="required">
				<xsd:annotation>
					<xsd:documentation>
						0=Error,
						1=Warning,
						2=Message (Successful),
						3=Fatal error
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="0"/>
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Messages">
		<xsd:annotation>
			<xsd:documentation>Result messages</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Message" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Account" type="typeAccount"/>
	<xsd:element name="Accounts">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Account" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Administration">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Name"/>
				<xsd:element name="AddressLine1" type="xsd:string" minOccurs="0"/>
				<xsd:element name="AddressLine2" type="xsd:string" minOccurs="0"/>
				<xsd:element name="AddressLine3" type="xsd:string" minOccurs="0"/>
				<xsd:element name="Postcode" type="xsd:string" minOccurs="0"/>
				<xsd:element name="City" type="xsd:string" minOccurs="0"/>
				<xsd:element ref="State" minOccurs="0"/>
				<xsd:element ref="Country" minOccurs="0"/>
				<xsd:element name="Currency" type="typeCurrency" minOccurs="0"/>
				<xsd:element ref="Phone" minOccurs="0"/>
				<xsd:element ref="PhoneExt" minOccurs="0"/>
				<xsd:element ref="Fax" minOccurs="0"/>
				<xsd:element ref="Email" minOccurs="0"/>
				<xsd:element ref="HomePage" minOccurs="0"/>
				<xsd:element name="VATNumber" type="xsd:string" minOccurs="0"/>
				<xsd:element name="ChamberOfCommerce" type="xsd:string" minOccurs="0"/>
				<xsd:element name="SBICode" minOccurs="0">
					<xsd:complexType>
						<xsd:attribute ref="code" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name ="ChamberOfCommerceEstablishment" type="xsd:string" minOccurs="0"/>
				<xsd:element name="ActivitySector" minOccurs="0">
					<xsd:complexType>
						<xsd:attribute ref="code" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="ActivitySubSector" minOccurs="0">
					<xsd:complexType>
						<xsd:attribute ref="code" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="CompanySize" minOccurs="0">
					<xsd:complexType>
						<xsd:attribute ref="code" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="BusinessType" minOccurs="0">
					<xsd:complexType>
						<xsd:attribute ref="code" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="SepaDDCreditorIdentifier" type="xsd:string" minOccurs="0"/>
				<xsd:element ref="StartDate" minOccurs="0"/>
				<xsd:element ref="EndDate" minOccurs="0"/>
				<xsd:element name="BlockingStatus" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							0=Not blocked,
							1=Backup,
							2=Conversion busy,
							3=Conversion shadow,
							4=Conversion waiting,
							5=Copy data waiting,
							6=Copy data busy
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="DigitalPostBoxes" type="typeDigitalPostBoxes" minOccurs="0"/>
				<xsd:element ref="UserAdministrations" minOccurs="0"/>
				<xsd:element ref="DivisionClassNames" minOccurs="0"/>
				<xsd:element ref="AdministrationTypes" minOccurs="0"/>
				<xsd:element name="EttSource" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							0=Not applicable,
							1=ProAcc PersProf,
							2=ProAcc Corporate,
							3=ProAcc Studio,
							4=Cubic,
							5=Globe,
							6=ExactOnLine,
							7=Winbooks,
							8=Auditfile - Twinfield,
							9=Auditfile - Microsoft Navision Attain,
							10=Auditfile - iMUIS,
							11=Auditfile - Reeleezee Audit,
							12=Auditfile - SAP SBO NL,
							13=Auditfile - Yuki,
							14=Auditfile - Snelstart
							<!-- Reserve [15 to 18] for dataswitcher conversion sources, not displaying it in documentation  -->
							<!-- 15=DataSwitcher - Vero -->
							<!-- 16=DataSwitcher - SageBob -->
							<!-- 17=DataSwitcher - ExpertM -->
							<!-- 18=DataSwitcher - Adsolut -->
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute ref="code">
				<xsd:annotation>
					<xsd:documentation>The internal unique code of the administration.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="number" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>The external code of the administration as shown in the administration card.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Administrations">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Administration" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="AR">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Account"/>
				<xsd:element name="OutstandingItems">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="OutstandingItem" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element ref="Description" minOccurs="0"/>
										<xsd:element ref="InvoiceDate"/>
										<xsd:element ref="DueDate"/>
										<xsd:element name="Currency" type="typeCurrency"/>
										<xsd:element name="InvoiceAmtFC" type="xsd:float"/>
										<xsd:element name="OutstandingAmtFC" type="xsd:float"/>
										<xsd:element name="InvoiceAmtDC" type="xsd:float"/>
										<xsd:element name="OutstandingAmtDC" type="xsd:float"/>
									</xsd:sequence>
									<xsd:attribute ref="number"/>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ARs">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="AR" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="AP">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Account"/>
				<xsd:element name="OutstandingItems">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="OutstandingItem" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element ref="YourRef" minOccurs="0"/>
										<xsd:element ref="Description" minOccurs="0"/>
										<xsd:element ref="InvoiceDate"/>
										<xsd:element ref="DueDate"/>
										<xsd:element name="Currency" type="typeCurrency"/>
										<xsd:element name="InvoiceAmtFC" type="xsd:float"/>
										<xsd:element name="OutstandingAmtFC" type="xsd:float"/>
										<xsd:element name="InvoiceAmtDC" type="xsd:float"/>
										<xsd:element name="OutstandingAmtDC" type="xsd:float"/>
									</xsd:sequence>
									<xsd:attribute ref="number"/>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="APs">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="AP" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="BalanceCostAnalysisType">
		<xsd:sequence>
			<xsd:element name="Costcenter" minOccurs="1" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence minOccurs="1">
						<xsd:element name="Costunit" minOccurs="1" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:sequence minOccurs="1">
									<xsd:element name="Debit" type="xsd:float"/>
									<xsd:element name="Credit" type="xsd:float"/>
								</xsd:sequence>
								<xsd:attribute ref="code"/>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
					<xsd:attribute ref="code"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="Balance">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Description"/>
				<xsd:element name="LeadCode" type="xsd:string"/>
				<xsd:element name="Years">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Year" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Open" type="xsd:float"/>
										<xsd:element name="Debit" type="xsd:float"/>
										<xsd:element name="Credit" type="xsd:float"/>
										<xsd:element name="Close" type="xsd:float"/>
										<xsd:element name="Periods" minOccurs="0">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="Period" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="Debit" type="xsd:float"/>
																<xsd:element name="Credit" type="xsd:float"/>
																<xsd:element name="CostAnalysis" type="BalanceCostAnalysisType" minOccurs="0" />
															</xsd:sequence>
															<xsd:attribute name="ReportingPeriod" type="xsd:integer"/>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="CostAnalysis" type="BalanceCostAnalysisType" minOccurs="0" />
										<xsd:element name="Groups" minOccurs="0">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="Group" maxOccurs="unbounded">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element ref="Journal" minOccurs="0"/>
																<xsd:element ref="Costcenter" minOccurs="0"/>
																<xsd:element name="Debit" type="xsd:float"/>
																<xsd:element name="Credit" type="xsd:float"/>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
									<xsd:attribute name="ReportingYear" type="xsd:integer"/>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="GLClassificationLinks" minOccurs="0" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="GLClassificationLink" minOccurs="0" maxOccurs="unbounded"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute ref="code"/>
			<xsd:attribute ref="balanceType"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Balances">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="CompanyData">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CompanyName" type="xsd:string"/>
							<xsd:element name="Address" type="xsd:string"/>
							<xsd:element name="PostalCode" type="xsd:string"/>
							<xsd:element name="City" type="xsd:string"/>
							<xsd:element ref="Country"/>
							<xsd:element name="ChamberOfCommerce" type="xsd:string"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="Balance" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="BankLink">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Account"/>
				<xsd:element ref="Mailbox"/>
				<xsd:element ref="BankAccount"/>
				<xsd:element name="SynchronizationCode" type="xsd:string"/>
				<xsd:element ref="Active" minOccurs="0"/>
				<xsd:element ref="Administration"/>
				<xsd:element name="TransactionsPermission" type="xsd:boolean"/>
				<xsd:element name="PaymentsPermission" type="xsd:boolean"/>
				<xsd:element name="CollectionsPermission" type="xsd:boolean"/>
				<xsd:element ref="SequenceNumber" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="BankLinks">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="BankLink" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Item" type="typeItem">
		<xsd:annotation>
			<xsd:documentation>Items are used if you have Purchase order, Sales order or Invoice functionality in your subscription</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="typeItem">
		<xsd:sequence minOccurs="0">
			<xsd:element ref="Description" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="Translations" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="ExtraDescription" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The extra description involving the Item</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="xsd:string">
							<xsd:attribute name="termid"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Barcode" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="IsSalesItem" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="IsStockItem" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="IsPurchaseItem" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="IsFractionAllowedItem" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="IsMakeItem" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="IsSubcontractedItem" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="IsTime" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="IsOnDemandItem" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="IsWebshopItem" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="CopyRemarks" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Copy sales remarks to sales lines</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IsSerialItem" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="IsBatchItem" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="IsTaxable" type="xsd:boolean" minOccurs="0"/>
			<xsd:element ref="Assortment" minOccurs="0"/>
			<xsd:element ref="GLRevenue" minOccurs="0"/>
			<xsd:element ref="GLCosts" minOccurs="0"/>
			<xsd:element name="GLRevenueWorkInProgress" type="typeGLAccount" minOccurs="0"/>
			<xsd:element name="GLCostsWorkInProgress" type="typeGLAccount" minOccurs="0"/>
			<xsd:element ref="GLPurchase" minOccurs="0"/>
			<xsd:element name="GLAsset" type="typeGLAccount" minOccurs="0"/>
			<xsd:element name="Sales" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="Price"/>
						<xsd:element ref="Unit" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Costs" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="Price"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element ref="DateStart" minOccurs="0"/>
			<xsd:element ref="DateEnd" minOccurs="0"/>
			<xsd:element name="Statistical" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Number" type="xsd:string"/>
						<xsd:element name="Units" type="xsd:float"/>
						<xsd:element name="Quantity" type="xsd:float"/>
						<xsd:element name="Value" type="xsd:float" minOccurs="0"/>
						<xsd:element name="NetWeight" type="xsd:float" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="TransportData" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="GrossWeight" type="xsd:float" minOccurs="0"/>
						<xsd:element name="NetWeight" type="xsd:float" minOccurs="0"/>
						<xsd:element name="NetWeightUnit" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>NetWeightUnit is only used with manufacturing packages</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ShapeCharacteristics" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:choice>
						<xsd:element name="Bar" type="BarShape" />
						<xsd:element name="Sheet" type="SheetShape" />
						<xsd:element name="CoilWireLength" type="CoilWireLengthShape" />
						<xsd:element name="CoilWireWeight" type="CoilWireWeightShape" />
						<xsd:element name="Volume" type="VolumeShape" />
					</xsd:choice>
				</xsd:complexType>
			</xsd:element>
			<xsd:element ref="Image" minOccurs="0"/>
			<xsd:element ref="Note" minOccurs="0"/>
			<xsd:element ref="ItemPrice" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>List of ItemPrices should be under a separate tag</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ItemCategory" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence minOccurs="0">
						<xsd:element ref="Description"/>
					</xsd:sequence>
					<xsd:attribute ref="number" use="required"/>
					<xsd:attribute ref="code" use="required"/>
					<xsd:attribute name="class"/>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ItemExtraFields" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence minOccurs="0">
						<xsd:element name="ExtraField1" minOccurs="0"/>
						<xsd:element name="ExtraField2" minOccurs="0"/>
						<xsd:element name="ExtraField3" minOccurs="0"/>
						<xsd:element name="ExtraField4" minOccurs="0"/>
						<xsd:element name="ExtraField5" minOccurs="0"/>
						<xsd:element name="ExtraField6" minOccurs="0"/>
						<xsd:element name="ExtraField7" minOccurs="0"/>
						<xsd:element name="ExtraField8" minOccurs="0"/>
						<xsd:element name="ExtraField9" minOccurs="0"/>
						<xsd:element name="ExtraField10" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ItemAccounts" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="ItemAccount" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ItemWarehouses" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="ItemWarehouse" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute ref="ID" use="optional"/>
		<xsd:attribute ref="code" use="required"/>
		<xsd:attribute ref="number" use="optional"/>
		<xsd:attribute name="searchcode" use="optional"/>
	</xsd:complexType>
	<xsd:complexType name="typeItems">
		<xsd:annotation>
			<xsd:documentation>represent a list of Item</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence minOccurs="0">
			<xsd:element ref="Item" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="ItemPrice">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Currency" type="typeCurrency"/>
				<xsd:element ref="Value"/>
				<xsd:element name="Type" type="xsd:token" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Type is no longer used</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="Unit"/>
				<xsd:element ref="UnitFactor"/>
				<xsd:element ref="DateStart"/>
				<xsd:element ref="DateEnd" minOccurs="0"/>
				<xsd:element ref="Quantity" minOccurs="0"/>
				<xsd:element ref="Country" minOccurs="0"/>
				<xsd:element name="Account" type="typeAccount" minOccurs="0"/>
				<xsd:element ref="Project" minOccurs="0"/>
				<xsd:element ref="Employee" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="type">
				<xsd:simpleType>
					<xsd:annotation>
						<xsd:documentation>
							1=Sales price,
							'S' is old style value for Sales price. If possible replace with value 1
						</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="S"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="leading">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="I"/>
						<xsd:enumeration value="U"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute ref="ID" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ItemWarehouse">
		<xsd:complexType>
			<xsd:all>
				<xsd:element ref="Item" minOccurs="0"/>
				<xsd:element ref="Warehouse" minOccurs="0"/>
				<xsd:element ref="StorageLocation" minOccurs="0"/>
				<xsd:element name="CountingCycle" minOccurs="0"/>
				<xsd:element name="NextCountingCycle" minOccurs="0"/>
				<xsd:element name="ReplenishmentType" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Description" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
						<xsd:attribute name="id" type="xsd:string"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="OrderPolicy" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Description" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
						<xsd:attribute name="id" type="xsd:string"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="SafetyStock" type="xsd:float" minOccurs="0"/>
				<xsd:element name="ReorderPoint" type="xsd:float"/>
				<xsd:element name="ReorderQuantity" minOccurs="0"/>
				<xsd:element name="MaximumStock" type="xsd:float" minOccurs="0"/>
				<xsd:element name="Period" minOccurs="0"/>
			</xsd:all>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ItemAccount">
		<xsd:complexType>
			<xsd:all>
				<xsd:element name="Account" type="typeAccount"/>
				<xsd:element name="IsPrimary" minOccurs="0"/>
				<xsd:element name="SupplierItemCode" minOccurs="0"/>
				<xsd:element name="Purchase" minOccurs="0">
					<xsd:complexType>
						<xsd:all>
							<xsd:element ref="Price" minOccurs="0"/>
							<xsd:element ref="Unit" minOccurs="0"/>
							<xsd:element ref="UnitFactor" minOccurs="0"/>
							<xsd:element name="LeadDays" minOccurs="0"/>
							<xsd:element name="MinimumReorderQuantity" minOccurs="0"/>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Note" minOccurs="0"/>
				<xsd:element name="CanDropShip" minOccurs="0">
					<xsd:simpleType>
						<xsd:annotation>
							<xsd:documentation>0=No, 1=Yes, 2=Optional</xsd:documentation>
						</xsd:annotation>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="0"/>
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="CountryOfOrigin" minOccurs="0"/>
			</xsd:all>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Items">
		<xsd:annotation>
			<xsd:documentation>Items are used if you have the invoice functionality in your subscription</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Item" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Document">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="Subject" type="xsd:string"/>
				<xsd:element name="EmailTextSubject" minOccurs="0"/>
				<xsd:element name="DDView" minOccurs="0"/>
				<xsd:element name="Publish" minOccurs="0">
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="0"/>
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Status" minOccurs="0">
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="20"/>
							<xsd:enumeration value="25"/>
							<xsd:enumeration value="30"/>
							<xsd:enumeration value="100"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element name="DocumentType" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Description" minOccurs="0"/>
						</xsd:sequence>
						<xsd:attribute ref="number" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="DocumentCategory" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence minOccurs="0">
							<xsd:element ref="Description"/>
						</xsd:sequence>
						<xsd:attribute ref="ID" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="MandatoryFeatureSets" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="MandatoryFeatureSet" minOccurs="1" maxOccurs="unbounded">
								<xsd:complexType mixed="true">
									<xsd:attribute name="code" type="xsd:integer" use="required"/>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="ForbiddenFeatureSets" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="ForbiddenFeatureSet" minOccurs="1" maxOccurs="unbounded">
								<xsd:complexType mixed="true">
									<xsd:attribute name="code" type="xsd:integer" use="required"/>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="Language" minOccurs="0"/>
				<xsd:element ref="Account" minOccurs="0"/>
				<xsd:element ref="Assortment" minOccurs="0"/>
				<xsd:element ref="Item" minOccurs="0"/>
				<xsd:element name="Asset" type="typeAsset" minOccurs="0"/>
				<xsd:element ref="Amount" minOccurs="0"/>
				<xsd:element ref="DateStart" minOccurs="0"/>
				<xsd:element ref="DateEnd" minOccurs="0"/>
				<xsd:element ref="Date" minOccurs="0"/>
				<xsd:element ref="ExpiryDate" minOccurs="0"/>
				<xsd:element name="Share" type="xsd:integer" minOccurs="0"/>
				<xsd:element ref="References" minOccurs="0"/>
				<xsd:element name="Body" minOccurs="0"/>
				<xsd:element name="PreviewBody" minOccurs="0"/>
				<xsd:element name="Images" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Image" maxOccurs="unbounded"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Attachments" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Attachment" maxOccurs="unbounded"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute ref="ID" use="optional"/>
			<xsd:attribute ref="number" use="optional"/>

		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Documents">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Document" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GLAccount" type="typeGLAccount"/>
	<xsd:element name="GLAccounts">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="GLAccount" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GLAccountCategory">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="GLAccountCategoryName" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Description"/>
						</xsd:sequence>
						<xsd:attribute ref="number" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="Description"/>
			</xsd:sequence>
			<xsd:attribute ref="number" use="required"/>
			<xsd:attribute ref="code" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GLAccountCategories">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="GLAccountCategory" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GLAccountClassifications">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="GLAccountScheme" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GLAccountScheme">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Description"/>
				<xsd:element ref="Translations" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="GLClassification" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="GLAccountCategory" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attribute ref="code" use="required"/>
			<xsd:attribute name="main" type="xsd:string" use="optional"/>
			<xsd:attribute name="type" type="xsd:string" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Property">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Description"/>
			</xsd:sequence>
			<xsd:attribute ref="code" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GLClassification">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Description"/>
				<xsd:element ref="Translations" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="GLClassification" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attribute ref="code" use="required"/>
			<xsd:attribute name="calculationType" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GLClassificationLink">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="GLAccountScheme"/>
				<xsd:element ref="GLClassification"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="AccountClassification">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Description"/>
				<xsd:element ref="Property"/>
			</xsd:sequence>
			<xsd:attribute name="SequenceNumber" use ="required"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GLTransactionLine">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Date"/>
				<xsd:element name="VATType" type="typeVATType" minOccurs="0"/>
				<xsd:element ref="FinYear" minOccurs="0"/>
				<xsd:element ref="FinPeriod" minOccurs="0"/>
				<xsd:element ref="GLAccount"/>
				<xsd:element ref="GLOffset" minOccurs="0"/>
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element ref="DueDate" minOccurs="0"/>
				<xsd:element ref="InvoiceDate" minOccurs="0"/>
				<xsd:element ref="Account" minOccurs="0"/>
				<xsd:element ref="TrackingNumber" minOccurs="0"/>
				<xsd:element ref="Costcenter" minOccurs="0"/>
				<xsd:element ref="Costunit" minOccurs="0"/>
				<xsd:element ref="Asset" minOccurs="0"/>
				<xsd:element ref="Item" minOccurs="0"/>
				<xsd:element ref="Quantity" minOccurs="0"/>
				<xsd:element ref="Amount" minOccurs="0"/>
				<xsd:element ref="ForeignAmount" minOccurs="0"/>
				<xsd:element ref="References" minOccurs="0"/>
				<xsd:element ref="OwnBankAccount" minOccurs="0"/>
				<xsd:element ref="BankAccount" minOccurs="0"/>
				<xsd:element name="MatchID" type="xsd:string" minOccurs="0"/>
				<xsd:element ref="Note" minOccurs="0"/>
				<xsd:element ref="Document" minOccurs="0"/>
				<xsd:element ref="Project" minOccurs="0"/>
				<xsd:element ref="Subscription" minOccurs="0" />
				<xsd:element name="IntraStat" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence minOccurs="0">
							<xsd:element name="StatisticalNumber" minOccurs="0"/>
							<xsd:element name="StatisticalValue" type="xsd:float" minOccurs="0"/>
							<xsd:element name="StatisticalQuantity" type="xsd:float" minOccurs="0"/>
							<xsd:element name="StatisticalNetWeight" type="xsd:float" minOccurs="0"/>
							<xsd:element name="System" minOccurs="0"/>
							<xsd:element name="TransactionA" minOccurs="0"/>
							<xsd:element name="TransactionB" minOccurs="0"/>
							<xsd:element name="TransportMethod" minOccurs="0"/>
							<xsd:element name="DeliveryTerm" minOccurs="0"/>
							<xsd:element name="Area" minOccurs="0"/>
							<xsd:element name="Reference" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="IsExtraDuty" type="xsd:boolean" minOccurs="0"/>
				<xsd:element name="ExtraDutyPercentage" minOccurs="0"/>
				<xsd:element name="ExtraDutyAmount" type="xsd:float" minOccurs="0"/>
				<xsd:element ref="DeferredFrom" minOccurs="0"/>
				<xsd:element ref="DeferredTo" minOccurs="0" />
				<xsd:element ref="IsSupplierPaymentReporting" minOccurs="0" />
				<xsd:element name="AlreadyDeclared" type="xsd:boolean" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>For internal use only. Indicates VAT for current transaction has been declared.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute ref="type"/>
			<xsd:attribute name="linetype">
				<xsd:annotation>
					<xsd:documentation>
						In case of type Opening balance (10), the possible values are:

						10=Cash,
						12=Bank,
						20=Accounts receivable,
						22=Accounts payable,
						24=VAT,
						26=Prepaid expenses,
						27=Accrued expenses,
						29=Income tax payable,
						30=Fixed assets,
						32=Other assets,
						35=Accumulated depreciations,
						40=Inventory,
						50=Capital stock,
						52=Retained earnings,
						55=Long term debt,
						60=Current portion of debt,
						90=General,
						110=Revenue,
						111=Cost of goods,
						120=Other costs,
						121=Sales, marketing general expenses,
						122=Depreciation costs,
						123=Research and development,
						125=Employee costs,
						130=Exceptional costs,
						140=Exceptional income,
						150=Income taxes,
						160=Interest income

						In case of asset transactions (type 70-76), the possible values are:

						0=Asset investment (used for type Asset investment (71), GLAssets transaction line),
						1=Asset depreciation (used for all asset transactions (70-76), GLDepreciationBS transaction line),
						2=Asset depreciation costs (used for types Asset depreciation (70), Asset revaluation (72), Asset split (74), Asset discontinue (75) and Asset sales (76), GLDepreciationPL transaction line),
						3=Asset temporary revenue (used for type Asset sales (76), GLSales transaction line),
						10=Asset opening balance investment (used for type Asset investment (71), GLAssets transaction line in opening entry),
						11=Asset opening balance depreciation (used for type Asset investment (71), GLDepreciationBS transaction line in opening entry),
						12=Asset opening balance revaluation (used for type Asset revaluation (72), GLRevaluationBS transaction line in opening entry)
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="0"/>
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
						<xsd:enumeration value="10"/>
						<xsd:enumeration value="11"/>
						<xsd:enumeration value="12"/>
						<xsd:enumeration value="20"/>
						<xsd:enumeration value="22"/>
						<xsd:enumeration value="24"/>
						<xsd:enumeration value="26"/>
						<xsd:enumeration value="27"/>
						<xsd:enumeration value="29"/>
						<xsd:enumeration value="30"/>
						<xsd:enumeration value="32"/>
						<xsd:enumeration value="35"/>
						<xsd:enumeration value="40"/>
						<xsd:enumeration value="50"/>
						<xsd:enumeration value="52"/>
						<xsd:enumeration value="55"/>
						<xsd:enumeration value="60"/>
						<xsd:enumeration value="90"/>
						<xsd:enumeration value="110"/>
						<xsd:enumeration value="111"/>
						<xsd:enumeration value="120"/>
						<xsd:enumeration value="121"/>
						<xsd:enumeration value="122"/>
						<xsd:enumeration value="123"/>
						<xsd:enumeration value="125"/>
						<xsd:enumeration value="130"/>
						<xsd:enumeration value="140"/>
						<xsd:enumeration value="150"/>
						<xsd:enumeration value="160"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="line" type="xsd:integer"/>
			<xsd:attribute name="offsetline" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>Only used for cash/bank entries. The sequence number of the offset line, starting at 1 for each bank line. This is the same number as in offsetline in the PaymentTerm.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="writeoffline" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>Only used for cash/bank entries. The sequence number of the write-off line in the current entry, the write-off offset line linked to it has the same number. This is the same number as in the write-off line in the PaymentTerm.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="status">
				<xsd:annotation>
					<xsd:documentation>20=Open, 50=Processed</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="20"/>
						<xsd:enumeration value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GLTransaction">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="TransactionType" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence minOccurs="0">
							<xsd:element ref="Description"/>
						</xsd:sequence>
						<xsd:attribute ref="number" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="Journal" minOccurs="0"/>
				<xsd:element ref="PaymentCondition" minOccurs="0"/>
				<xsd:element ref="WithholdingTax" minOccurs="0"/>
				<xsd:element name="GAccountAmountFC" minOccurs="0"/>
				<xsd:element name="ExternalLinks" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="ExternalLink" minOccurs="0" maxOccurs="1">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Description" minOccurs="0"/>
										<xsd:element name="Reference" minOccurs="1"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="Date" minOccurs="0"/>
				<xsd:element ref="FinYear" minOccurs="0"/>
				<xsd:element ref="FinPeriod" minOccurs="0"/>
				<xsd:element name="OpeningBalance" type="xsd:float" minOccurs="0"/>
				<xsd:element name="ClosingBalance" type="xsd:float" minOccurs="0"/>
				<xsd:element ref="Document" minOccurs="0"/>
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element ref="Account" minOccurs="0"/>
				<xsd:element ref="GLTransactionLine" maxOccurs="unbounded"/>
				<xsd:element ref="PaymentTerms" minOccurs="0"/>
				<xsd:element name="RunOfficialReturnCreatorOnConversion" type="xsd:boolean" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>For internal use only. Indicates last entry of import process.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="entry" type="xsd:integer" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GLTransactions">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="GLTransaction" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Indicators">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Indicator" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Indicator">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="Description" type="xsd:string" minOccurs="0"/>
				<xsd:element name="Active" type="xsd:integer" minOccurs="0"/>
				<xsd:element name="Severity" type="xsd:integer" minOccurs="0"/>
				<xsd:element name="Type" type="xsd:integer" minOccurs="0"/>
				<xsd:element name="Classification" type="xsd:short" minOccurs="0"/>
				<xsd:element ref="IndicatorValidations" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="ID" type="xsd:string"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="IndicatorValidations">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="IndicatorValidation" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="IndicatorValidation">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="Sequence" type="xsd:integer" minOccurs="0"/>
				<xsd:element name="GLAccount" type="typeGLAccount" minOccurs="0"/>
				<xsd:element name="Element" minOccurs="0">
					<xsd:complexType>
						<xsd:attribute ref="code" use="required"/>
						<xsd:attribute name="namespace" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="Journals" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="JournalTypes" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="GLAccounts" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="VATs" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="VATBoxes" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Operator" minOccurs="0">
					<xsd:simpleType>
						<xsd:annotation>
							<xsd:documentation>1=Equal to, 2=Unequal to, 3=Greater than, 4=Greater than or equal to, 5=Less than, 6=Less than or equal to, 7=Between, 8=Percentage greater than, 9=Percentage less than, 10=From GL Account, 11=No VAT Code used, 12=No VAT box used</xsd:documentation>
						</xsd:annotation>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
							<xsd:enumeration value="3"/>
							<xsd:enumeration value="4"/>
							<xsd:enumeration value="5"/>
							<xsd:enumeration value="6"/>
							<xsd:enumeration value="7"/>
							<xsd:enumeration value="8"/>
							<xsd:enumeration value="9"/>
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="11"/>
							<xsd:enumeration value="12"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="ValueFrom" type="xsd:float" minOccurs="0"/>
				<xsd:element name="ValueTo" type="xsd:float" minOccurs="0"/>
				<xsd:element name="StatusPassed" type="xsd:integer" minOccurs="0"/>
				<xsd:element name="StatusFailed" type="xsd:integer" minOccurs="0"/>
				<xsd:element name="CreateSignal" type="xsd:integer" minOccurs="0"/>
				<xsd:element name="SignalMessage" type="xsd:string" minOccurs="0"/>
				<xsd:element name="AfterEntry" type="xsd:boolean" minOccurs="0"/>
				<xsd:element name="Cumulative" type="xsd:boolean" minOccurs="0"/>
				<xsd:element name="Reverse" type="xsd:boolean" minOccurs="0"/>
				<xsd:element name="CompareWith" minOccurs="0">
					<xsd:simpleType>
						<xsd:annotation>
							<xsd:documentation>
								If Indicator Type is 6 (Difference result in percentage) then: 1=Last year, 2=Last year until period
								If Indicator Type is 29 (G/L account difference by period) then: 1=Last year, 2=Last year until period, 4=Previous period, 5=Same period last year
							</xsd:documentation>
						</xsd:annotation>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value=""/>
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
							<xsd:enumeration value="4"/>
							<xsd:enumeration value="5"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="AccountType" minOccurs="0">
					<xsd:simpleType>
						<xsd:annotation>
							<xsd:documentation>
								If Indicator Type is 6 (Difference result in percentage) then: 1=Revenue, 2=Costs, 3=Result
							</xsd:documentation>
						</xsd:annotation>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value=""/>
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
							<xsd:enumeration value="3"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Threshold" minOccurs="0">
					<xsd:simpleType>
						<xsd:union memberTypes="xsd:float">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value=""/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:union>
					</xsd:simpleType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="WithholdingTaxSales">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Amount" minOccurs="0"/>
				<xsd:element name="Percentage" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="EntryDiscount">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="AmountInclVAT" minOccurs="0"/>
				<xsd:element name="AmountExclVAT" minOccurs="0"/>
				<xsd:element name="Percentage" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Invoice">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Journal" minOccurs="0"/>
				<xsd:element ref="OrderDate" minOccurs="0"/>
				<xsd:element name="InvoiceDate" type="xsd:date" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							Invoice date: YYYY-MM-DD.
							Only used for export. For imported invoices the invoice date will be determined at final printing.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="DueDate" type="xsd:date" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							Due date: YYYY-MM-DD.
							Only used for export. For imported invoices the due date will be calculated based on the payment condition.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element ref="YourRef" minOccurs="0"/>
				<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
				<xsd:element name="OrderedBy" type="typeAccount"/>
				<xsd:element name="DeliverTo" type="typeAccount" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Only used for export. For imported sales orders the deliver to account is set to the order account. (The same as SalesOrder)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="DeliveryAddress" minOccurs="0"/>
				<xsd:element name="InvoiceTo" type="typeAccount"/>
				<xsd:element ref="Contact" minOccurs="0"/>
				<xsd:element ref="Warehouse" minOccurs="0"/>
				<xsd:element ref="PaymentCondition" minOccurs="0"/>
				<xsd:element name="DDMandate" type="typeDDMandate" minOccurs="0"/>
				<xsd:element ref="ForeignAmount" minOccurs="0"/>
				<xsd:element ref="GAccountAmount" minOccurs="0" />
				<xsd:element ref="Document" minOccurs="0"/>
				<xsd:element ref="ShippingMethod" minOccurs="0"/>
				<xsd:element name="SalesPerson" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="FullName" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
						<xsd:attribute name="id" type="xsd:string"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="SalesChannel" type="typeSalesChannel" minOccurs="0"/>
				<xsd:element ref="EntryDiscount" minOccurs="0"/>
				<xsd:element name="PaymentReference" type="xsd:string" minOccurs="0"/>
				<xsd:element name="IsExtraDuty" type="xsd:boolean" minOccurs="0"/>
				<xsd:element ref="WithholdingTaxSales" minOccurs="0" />
				<xsd:element name="Incoterm" type="typeIncoterm" minOccurs="0"/>
				<xsd:element name="InvoiceLine" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Description" minOccurs="0"/>
							<xsd:element ref="Item"/>
							<xsd:element ref="Quantity" minOccurs="0"/>
							<xsd:element ref="Unit" minOccurs="0"/>
							<xsd:element name="UnitPrice" type="typePrice" minOccurs="0"/>
							<xsd:element name="NetPrice" type="typePrice" minOccurs="0"/>
							<xsd:element ref="ForeignAmount" minOccurs="0"/>
							<xsd:element name="DiscountPercentage" type="typePercentage" minOccurs="0"/>
							<xsd:element ref="Costcenter" minOccurs="0"/>
							<xsd:element ref="Costunit" minOccurs="0"/>
							<xsd:element ref="Note" minOccurs="0"/>
							<xsd:element ref="StartDate" minOccurs="0"/>
							<xsd:element ref="EndDate" minOccurs="0"/>
							<xsd:element ref="Project" minOccurs="0"/>
							<xsd:element ref="Employee" minOccurs="0"/>
							<xsd:element ref="GLAccount" minOccurs="0"/>
							<xsd:element ref="TaxSchedule" minOccurs="0"/>
							<xsd:element name="DeliveryDate" type="xsd:date" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>
										This is used for VAT on prepayments if no sales order is used in your license.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ExtraDutyPercentage" type="typePercentage" minOccurs="0"/>
							<xsd:element name="ExtraDutyAmount" type="xsd:float" minOccurs="0"/>
							<xsd:choice minOccurs="0">
								<xsd:element name ="Activity" minOccurs="0">
									<xsd:complexType>
										<xsd:attribute name="ID" type="xsd:string" use="required"/>
									</xsd:complexType>
								</xsd:element>
								<xsd:element name ="Expense" minOccurs="0">
									<xsd:complexType>
										<xsd:attribute name="ID" type="xsd:string" use="required"/>
									</xsd:complexType>
								</xsd:element>
							</xsd:choice>
						</xsd:sequence>
						<xsd:attribute name="line" type="xsd:integer" use="optional"/>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="ordernumber" use="optional">
				<xsd:annotation>
					<xsd:documentation>The order number can be used multiple times. If an invoice with this order number already exists or was already finally printed, a new invoice with the same order number will be created. When no order number is supplied, a new invoice with the next available order number will be created.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="invoicenumber" use="optional"/>
			<xsd:attribute name="type" use="optional">
				<xsd:annotation>
					<xsd:documentation>8020=Normal, 8021=Credit note, 8023=Direct sales invoice, 8024=Direct credit note. Only used for export, if not filled, then imported invoices always get type 8020. If filled with 8023/8024, then imported invoices always get type 8023.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="8020"/>
						<xsd:enumeration value="8021"/>
						<xsd:enumeration value="8023"/>
						<xsd:enumeration value="8024"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="status" use="optional">
				<xsd:annotation>
					<xsd:documentation>20=Open, 50=historical/printed. Only used for export, imported invoices always get status 20.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="20"/>
						<xsd:enumeration value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Invoices">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Invoice" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="PurchaseOrder">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="Supplier" type="typeAccount"/>
				<xsd:element ref="Warehouse" minOccurs="0"/>
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element ref="OrderDate" minOccurs="0"/>
				<xsd:element ref="ReceiptDate" minOccurs="0"/>
				<xsd:element ref="YourRef" minOccurs="0"/>
				<xsd:element ref="PaymentCondition" minOccurs="0"/>
				<xsd:element ref="ForeignAmount" minOccurs="0"/>
				<xsd:element ref="ShippingMethod" minOccurs="0"/>
				<xsd:element name="PurchaseAgent">
					<xsd:annotation>
						<xsd:documentation>For imported purchase orders, the purchase agent must exist as a user to be included on the purchase order. This import will not create new users.</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="FullName" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
						<xsd:attribute name="id" type="xsd:string"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="Document" minOccurs="0"/>
				<xsd:element name="Incoterm" type="typeIncoterm" minOccurs="0"/>
				<xsd:element name="PurchaseOrderLine" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Description" minOccurs="0"/>
							<xsd:element ref="Item"/>
							<xsd:element ref="Quantity" minOccurs="0"/>
							<xsd:element ref="Unit" minOccurs="0"/>
							<xsd:element name="UnitPrice" type="typePrice" minOccurs="0"/>
							<xsd:element name="NetPrice" type="typePrice" minOccurs="0"/>
							<xsd:element ref="ForeignAmount" minOccurs="0"/>
							<xsd:element name="DiscountPercentage" type="typePercentage" minOccurs="0"/>
							<xsd:element ref="Costcenter" minOccurs="0"/>
							<xsd:element ref="Costunit" minOccurs="0"/>
							<xsd:element ref="Remarks" minOccurs="0"/>
							<xsd:element name="SupplierItemCode" type="xsd:string" minOccurs="0"/>
							<xsd:element ref="ReceiptDate" minOccurs="0"/>
							<xsd:element ref="Country" minOccurs="0"/>
							<xsd:element ref="Project" minOccurs="0"/>
							<xsd:element name ="Expense" minOccurs="0">
								<xsd:complexType>
									<xsd:attribute name="ID" type="xsd:string" use="required"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Rebill" type="xsd:boolean" minOccurs="0"/>
						</xsd:sequence>
						<xsd:attribute name="line" type="xsd:integer" use="optional"/>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="ordernumber" use="optional">
				<xsd:annotation>
					<xsd:documentation>The order number can be used multiple times. If a purchase order with this order number already exists, a new purchase order with the same order number will be created. When no order number is supplied, a new purchase order with the next available order number will be created.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="status" use="optional">
				<xsd:annotation>
					<xsd:documentation>10=Open, 20=Partial, 30=Complete, 40=Canceled. Only used for export, imported purchase orders always get status 10.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="10"/>
						<xsd:enumeration value="20"/>
						<xsd:enumeration value="30"/>
						<xsd:enumeration value="40"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="PurchaseOrders">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="PurchaseOrder" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SalesOrder">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="OrderDate" minOccurs="0"/>
				<xsd:element name="DeliveryDate" type="xsd:date" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							Due date: YYYY-MM-DD.
							Only used for export. For imported sales orders the delivery date will be calculated based on the minimum delivery date of the lines.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
