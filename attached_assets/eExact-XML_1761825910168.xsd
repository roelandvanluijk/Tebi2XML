	<!-- Payroll/HRM - End -->
	<xsd:element name="PaymentConditions">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="PaymentCondition" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="BICCode" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>The Bank Identification Code</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BankName" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>The name of the bank</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="OrderNumber" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>The order number</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IBAN" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>The International Bank Account Number (IBAN)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PaymentCondition" type="PaymentCondition">
		<xsd:annotation>
			<xsd:documentation>
				During import of financial entries, the payment condition will first be retrieved
				from the payment term element. If not found there, the payment condition from the element GLTransaction will be used.
				When not found, the default from the administration settings will be used.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="PaymentCondition">
		<xsd:sequence minOccurs="0">
			<xsd:element ref="Description"/>
			<xsd:element ref="Translations" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="PaymentDiscountType" default="B" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="B"/>
						<xsd:enumeration value="K"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PaymentMethod" default="B" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="B"/>
						<xsd:enumeration value="C"/>
						<xsd:enumeration value="E"/>
						<xsd:enumeration value="H"/>
						<xsd:enumeration value="I"/>
						<xsd:enumeration value="K"/>
						<xsd:enumeration value="R"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PaymentEndOfMonths" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="PaymentDays" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="DiscountPaymentDays" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="DiscountPercentage" type="typePercentage" minOccurs="0"/>
			<xsd:element name="DiscountCalculation" default="E" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="E"/>
						<xsd:enumeration value="I"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="VATCalculation" default="E" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="E"/>
						<xsd:enumeration value="I"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="CreditManagementScenario" type="typeCreditManagementScenario" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute ref="code" use="required"/>
	</xsd:complexType>
	<xsd:element name="FinYears">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="FinYear" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<!-- Settings type Start -->
	<xsd:complexType name="typeSettings">
		<xsd:sequence>
			<xsd:element name="Setting" type="typeSetting" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="typeSetting">
		<xsd:sequence>
			<xsd:element name="Description" type="typeDescription" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="Suffix" type="typeDescription" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="FeatureSets" type="typeFeatureSets" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
		<xsd:attribute name="SettingName" type="typeString60" use="required"/>
		<xsd:attribute name="Value"/>
		<xsd:attribute name="ValueType" type="typeSettingValueEnum"/>
		<xsd:attribute name="Legislation" type="typeLegislation"/>
		<xsd:attribute name="TableName" type="typeString60"/>
		<xsd:attribute name="TemplateOnceOnly" type="xsd:integer"/>
	</xsd:complexType>
	<xsd:simpleType name="typeSettingValueEnum">
		<xsd:annotation>
			<xsd:documentation>
				0=StringValue,
				1=LongValue,
				2=DoubleValue,
				3=DateValue,
				4=XMLValue,
				5=GuidValue,
				6=BinaryValue
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="0"/>
			<xsd:enumeration value="1"/>
			<xsd:enumeration value="2"/>
			<xsd:enumeration value="3"/>
			<xsd:enumeration value="4"/>
			<xsd:enumeration value="5"/>
			<xsd:enumeration value="6"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Settings type End -->

	<xsd:element name="Titles">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Title" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:simpleType name="typeVATType">
		<xsd:annotation>
			<xsd:documentation>
				The VAT type determines the type of the amount in relation to VAT returns.

				For VAT lines of bank, cash and general entries the VAT type is mandatory.
				For other lines and other entries, if no VAT type is supplied, the VAT type will be determined based on the type of entry, type of G/L account, and type of VAT code.

				The following values are supported for base amounts:

				V - Sales basis
				X - Credit note sales basis
				B - Return sales invoice basis
				I - Purchase basis
				W - Credit note purchase basis
				T - Return purchase invoice received basis

				E - Provision for VAT margin: Sales basis
				G - Provision for VAT margin: Sales basis for credit note
				J - Provision for VAT margin: Purchase basis
				K - Provision for VAT margin: Purchase basis for credit note

				1 - Provision sales basis
				3 - Provision credit note sales basis
				5 - Provision purchase basis
				7 - Provision credit note purchase basis

				The following values are supported for VAT amounts:

				A - Sales VAT to pay
				Z - Credit note sales VAT to claim
				C - Return Sales invoice VAT to claim
				O - Purchase VAT to claim
				Y - Credit note purchase VAT to pay
				U - Return purchase invoice received VAT to pay

				P - Purchase VAT to pay
				Q - Credit note purchase VAT to claim
				D - Return purchase invoice received VAT to claim

				N - Purchase non-deductible
				M - Credit note purchase non-deductible

				9 - Sales based on Cash system
				L - Purchase based on Cash system

				R - Extra duty to pay
				D - Credit note extra duty to claim

				F - Provision for VAT margin: Sales VAT to pay
				H - Provision for VAT margin: Sales VAT to claim for credit note

				2 - Provision sales VAT to pay
				4 - Provision credit note sales VAT to claim
				6 - Provision purchase VAT to claim
				8 - Provision credit note purchase VAT to pay
				
				- - Credit note purchase private use
				+ - Purchase private use

				If no VAT code is used, or no VAT is involved, the VAT type must be set to S (No VAT).
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="1"/>
			<xsd:enumeration value="2"/>
			<xsd:enumeration value="3"/>
			<xsd:enumeration value="4"/>
			<xsd:enumeration value="5"/>
			<xsd:enumeration value="6"/>
			<xsd:enumeration value="7"/>
			<xsd:enumeration value="8"/>
			<xsd:enumeration value="9"/>
			<xsd:enumeration value="A"/>
			<xsd:enumeration value="B"/>
			<xsd:enumeration value="C"/>
			<xsd:enumeration value="D"/>
			<xsd:enumeration value="E"/>
			<xsd:enumeration value="F"/>
			<xsd:enumeration value="G"/>
			<xsd:enumeration value="H"/>
			<xsd:enumeration value="I"/>
			<xsd:enumeration value="J"/>
			<xsd:enumeration value="K"/>
			<xsd:enumeration value="L"/>
			<xsd:enumeration value="M"/>
			<xsd:enumeration value="N"/>
			<xsd:enumeration value="O"/>
			<xsd:enumeration value="P"/>
			<xsd:enumeration value="Q"/>
			<xsd:enumeration value="R"/>
			<xsd:enumeration value="S"/>
			<xsd:enumeration value="T"/>
			<xsd:enumeration value="U"/>
			<xsd:enumeration value="V"/>
			<xsd:enumeration value="W"/>
			<xsd:enumeration value="X"/>
			<xsd:enumeration value="Y"/>
			<xsd:enumeration value="Z"/>
			<xsd:enumeration value="-"/>
			<xsd:enumeration value="+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="typeUse">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="0"/>
			<xsd:enumeration value="1"/>
			<xsd:enumeration value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Asset module -->
	<!-- Asset elements list -->
	<xsd:element name="Assets">
		<xsd:annotation>
			<xsd:documentation>Assets elements list</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Asset" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- Asset group element list -->
	<xsd:element name="AssetGroups">
		<xsd:annotation>
			<xsd:documentation>Asset groups elements list</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="AssetGroup" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- Asset depreciation method element list -->
	<xsd:element name="DepreciationMethods">
		<xsd:annotation>
			<xsd:documentation>Asset depreciation methods elements list</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="DepreciationMethod" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- Asset element -->
	<xsd:element name="Asset" type="typeAsset"/>
	<xsd:element name="ParentAsset" type="typeAsset"/>
	<!-- AssetGroup element -->
	<xsd:element name="AssetGroup" type="typeAssetGroup"/>
	<!-- AssetDepreciationMethod element -->
	<xsd:element name="DepreciationMethod" type="typeDepreciationMethod"/>
	<xsd:element name="DepreciationInterval">
		<xsd:annotation>
			<xsd:documentation>Values P - Period, Y - Year.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:token">
				<xsd:enumeration value="P"/>
				<xsd:enumeration value="Y"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<!-- MatchSets -->
	<xsd:element name="MatchSets">
		<xsd:annotation>
			<xsd:documentation>List of matching definition</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="MatchSet" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- MatchSet -->
	<xsd:element name="MatchSet">
		<xsd:annotation>
			<xsd:documentation>
				One matching definition. If GLAccount is of type 20 - Accounts receivable or type = 22 - Accounts payable, the Account is mandatory.
				If GLAccount is not of type type 20 - Accounts receivable or type = 22 - Accounts payable, it should see Matching property set to true.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="GLAccount" type="typeGLAccount"/>
				<xsd:element name="Account" type="typeAccount" minOccurs="0"/>
				<xsd:element name="MatchLines">
					<xsd:annotation>
						<xsd:documentation>Matching lines enumeration.</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="MatchLine" maxOccurs="unbounded"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="WriteOff" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="SBICodeType">
		<xsd:sequence>
			<xsd:element ref="Description" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="code" type="xsd:string" use="required"/>
	</xsd:complexType>
	<!-- MatchLine -->
	<xsd:element name="MatchLine">
		<xsd:annotation>
			<xsd:documentation>One matching line identification. (finyear=Financial year number; finperiod=Financial period number; journal=Journal code; entry=Entry number; amountdc=Amount of the transaction in division currency)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="finyear" type="xsd:integer" use="required"/>
			<xsd:attribute name="finperiod" type="xsd:integer" use="required"/>
			<xsd:attribute name="journal" type="xsd:string" use="required"/>
			<xsd:attribute name="entry" type="xsd:integer" use="required"/>
			<xsd:attribute name="amountdc" type="xsd:float" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<!-- WriteOff -->
	<xsd:element name="WriteOff">
		<xsd:annotation>
			<xsd:documentation>Action take by the application if the total amount differs from zero. You can specify one of the following types: 0 = None.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="GLAccount" type="typeGLAccount" minOccurs="0"/>
				<xsd:element ref="Description" minOccurs="0"/>
				<xsd:element ref="FinYear" minOccurs="0"/>
				<xsd:element ref="FinPeriod" minOccurs="0"/>
				<xsd:element ref="Date" minOccurs="0"/>
				<xsd:element name="VATCorrection" type="xsd:boolean" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="type" use="required">
				<xsd:annotation>
					<xsd:documentation>0 = Balance; 1 = Discount; 3 = PaymentDifference; 4 = ExchangeRateDifference</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="0"/>
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="3"/>
						<xsd:enumeration value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- Marketing - Begin -->
	<xsd:complexType name="ActivitySectorType">
		<xsd:sequence>
			<xsd:element ref="Description" minOccurs="0"/>
