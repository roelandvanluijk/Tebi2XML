				<xsd:element name="PeriodClosingValidation" minOccurs="0">
					<xsd:simpleType>
						<xsd:annotation>
							<xsd:documentation>
								1	All sales invoice entry proposals have been processed
								2	All purchase invoice entry proposals have been processed
								3	All bank statements have been processed
								4	All missing document requests have been processed
								5	All questions about entries are answered
								6	All journal entries were processed in the right financial period
								7	The deferred revenue G/L account will end with a balance of  0
								8	The deferred costs G/L account will end with a balance of  0
								9	All previous VAT returns have been submitted
								11	The G/L account debtors correspond with the outstanding receivables list
								12	The G/L account creditors correspond with the outstanding payables list
							</xsd:documentation>
						</xsd:annotation>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
							<xsd:enumeration value="3"/>
							<xsd:enumeration value="4"/>
							<xsd:enumeration value="5"/>
							<xsd:enumeration value="6"/>
							<xsd:enumeration value="7"/>
							<xsd:enumeration value="8"/>
							<xsd:enumeration value="9"/>
							<xsd:enumeration value="11"/>
							<xsd:enumeration value="12"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Category" minOccurs="0">
					<xsd:simpleType>
						<xsd:annotation>
							<xsd:documentation>
								1	General
								2	Intangible assets
								3	Tangible assets
								4	Financial assets
								5	Stocks
								6	Receivables and accruals
								7	Securities
								8	Cash and banks
								9	Equity
								10	Provisions
								11	Non-current liabilities
								12	Current liabilities and accruals
								13	Taxes and social security contributions
								14	Revenue and costs of sales
								15	Other revenue
								16	Payroll costs
								17	Depreciations
								18	Other costs
								19	Financial income and expenses
								20	Others
								21	Balance dependency
							</xsd:documentation>
						</xsd:annotation>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
							<xsd:enumeration value="3"/>
							<xsd:enumeration value="4"/>
							<xsd:enumeration value="5"/>
							<xsd:enumeration value="6"/>
							<xsd:enumeration value="7"/>
							<xsd:enumeration value="8"/>
							<xsd:enumeration value="9"/>
							<xsd:enumeration value="10"/>
							<xsd:enumeration value="11"/>
							<xsd:enumeration value="12"/>
							<xsd:enumeration value="13"/>
							<xsd:enumeration value="14"/>
							<xsd:enumeration value="15"/>
							<xsd:enumeration value="16"/>
							<xsd:enumeration value="17"/>
							<xsd:enumeration value="18"/>
							<xsd:enumeration value="19"/>
							<xsd:enumeration value="20"/>
							<xsd:enumeration value="21"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element ref="TaxonomyElements" minOccurs="0"/>
				<xsd:element name="PeriodClosingInformationType" minOccurs="0">
					<xsd:simpleType>
						<xsd:annotation>
							<xsd:documentation>
								1	Status and balance of cash accounts
								2	Status and balance of bank accounts
							</xsd:documentation>
						</xsd:annotation>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element ref="Indicator" minOccurs="0"/>
				<xsd:element ref="PeriodClosingCheckMultiSelectOptions" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="ID" type="xsd:string"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="TaxonomyElements">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="TaxonomyElement" type="xsd:string" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="PeriodClosingCheckMultiSelectOptions">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="PeriodClosingCheckMultiSelectOption" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="PeriodClosingCheckMultiSelectOption">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="Code" type="xsd:integer" minOccurs="1"/>
				<xsd:element name="Description" type="xsd:string" minOccurs="1"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
		<!-- ClientAcceptanceTemplates Start-->
	<xsd:element name="ClientAcceptanceTemplates">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ClientAcceptanceTemplate" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- ClientAcceptanceTemplates Ends-->
	<!-- ClientAcceptanceTemplate Starts-->
	<xsd:element name="ClientAcceptanceTemplate">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="Description" minOccurs="1">
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="xsd:string">
								<xsd:attribute name="termid" type="xsd:integer"/>
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Code" minOccurs="1">
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="xsd:string">
								<xsd:attribute name="termid" type="xsd:integer"/>
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="StartDate" type="xsd:date" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation>YYYY-MM-DD</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="EndDate" minOccurs="0">
					<xsd:simpleType>
						<xsd:annotation>
							<xsd:documentation>YYYY-MM-DD</xsd:documentation>
						</xsd:annotation>
						<xsd:union memberTypes="xsd:date">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value=""/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:union>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
				<xsd:element ref="ClientAcceptanceTemplateQuestions" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="ID" type="xsd:string"/>
		</xsd:complexType>
	</xsd:element>
	<!-- ClientAcceptanceTemplate Ends-->
	<!-- ClientAcceptanceTemplateQuestions Starts-->
	<xsd:element name="ClientAcceptanceTemplateQuestions">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ClientAcceptanceTemplateQuestion" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ClientAcceptanceTemplateQuestion">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element name="Level1" type="xsd:integer" minOccurs="1"/>
				<xsd:element name="Level2" type="xsd:integer" minOccurs="0"/>
				<xsd:element name="ShowInState" minOccurs="0">
					<xsd:simpleType>
						<xsd:annotation>
							<xsd:documentation>0=No, 1=Yes</xsd:documentation>
						</xsd:annotation>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="0"/>
							<xsd:enumeration value="1"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="LinkedClientAcceptanceQuestionLevel" type="xsd:string" minOccurs="0"/>
				<xsd:element ref="ClientAcceptanceQuestion" minOccurs="1"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ClientAcceptanceQuestion">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
			<xsd:element name="Code" minOccurs="1">
				<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="xsd:string">
								<xsd:attribute name="termid" type="xsd:integer"/>
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Description" minOccurs="1">
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="xsd:string">
								<xsd:attribute name="termid" type="xsd:integer"/>
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="AnswerDataType" minOccurs="1">
					<xsd:simpleType>
						<xsd:annotation>
							<xsd:documentation>
								1=Text: Single line
								2=Number
								3=Amount
								4=Date
								5=Text: Multiple lines
								6=Date range
								11=E-mail address
								13=IBAN
								14=BSN
								15=RSIN
								16=CoC-number
								21=YesNo
								61=File
								62=Files
								63=Address
								64=Multiple options
								101=Section
								103=Explanation
								104=Attachment
							</xsd:documentation>
						</xsd:annotation>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="1"/>
							<xsd:enumeration value="2"/>
							<xsd:enumeration value="3"/>
							<xsd:enumeration value="4"/>
							<xsd:enumeration value="5"/>
							<xsd:enumeration value="6"/>
							<xsd:enumeration value="11"/>
							<xsd:enumeration value="13"/>
							<xsd:enumeration value="14"/>
							<xsd:enumeration value="15"/>
							<xsd:enumeration value="16"/>
							<xsd:enumeration value="21"/>
							<xsd:enumeration value="61"/>
							<xsd:enumeration value="62"/>
							<xsd:enumeration value="63"/>
							<xsd:enumeration value="64"/>
							<xsd:enumeration value="101"/>
							<xsd:enumeration value="103"/>
							<xsd:enumeration value="104"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<!-- <xsd:element ref="ClientAcceptanceMultiSelectOptions" minOccurs="0"/> -->
			</xsd:sequence>
			<xsd:attribute name="ID" type="xsd:string"/>
		</xsd:complexType>
	</xsd:element>
	<!-- ClientAcceptanceTemplateQuestions Ends-->
</xsd:schema>
