						<xsd:element name="TransactionB" minOccurs="0"/>
						<xsd:element name="TransportMethod" minOccurs="0"/>
						<xsd:element name="DeliveryTerm" minOccurs="0"/>
						<xsd:element name="Area" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
