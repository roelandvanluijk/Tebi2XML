			<xsd:element name="Classification2" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Classification3" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Classification4" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Classification5" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Classification6" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Classification7" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Classification8" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Hyperlink" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Notes" type="xsd:string" minOccurs="0"/>
