			</xsd:sequence>
			<xsd:attribute name="sign">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="P"/>
						<xsd:enumeration value="N"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="type">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="A"/>
						<xsd:enumeration value="D"/>
						<xsd:enumeration value="I"/>
						<xsd:enumeration value="M"/>
						<xsd:enumeration value="N"/>
						<xsd:enumeration value="O"/>
						<xsd:enumeration value="P"/>
						<xsd:enumeration value="Q"/>
						<xsd:enumeration value="R"/>
						<xsd:enumeration value="S"/>
						<xsd:enumeration value="T"/>
						<xsd:enumeration value="V"/>
						<xsd:enumeration value="W"/>
						<xsd:enumeration value="X"/>
						<xsd:enumeration value="Y"/>
						<xsd:enumeration value="Z"/>
						<xsd:enumeration value="J"/>
						<xsd:enumeration value="K"/>
						<xsd:enumeration value="L"/>
						<xsd:enumeration value="9"/>
						<xsd:enumeration value="-"/>
						<xsd:enumeration value="+"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="VATGLAccountType">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="D"/>
						<xsd:enumeration value="G"/>
						<xsd:enumeration value="I"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="BoxType">
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="B"/>
						<xsd:enumeration value="V"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="TaxReturnType">
				<xsd:annotation>
					<xsd:documentation>
						100 - VAT return (NL)
						200 - VAT return (BE)
						400 - VAT return (AN)
						500 - VAT return (DE)
						600 - VAT return 2014 (LU)
						601 - VAT return 2015 (LU)
						602 - VAT return 2019 (LU)
						603 - VAT return 2023 (LU)
						604 - VAT return 2024 (LU)
						1520 - Annual VAT Return - FULL (LU)
						700 - VAT return (GB)
						800 - Rgime TVA rel normal - CA3 (FR)
						801 - Rgime TVA Simplifi - CA12 (FR)
						900 - VAT return (ES)
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="100"/>
						<xsd:enumeration value="200"/>
						<xsd:enumeration value="400"/>
						<xsd:enumeration value="500"/>
						<xsd:enumeration value="600"/>
						<xsd:enumeration value="601"/>
						<xsd:enumeration value="602"/>
						<xsd:enumeration value="603"/>
						<xsd:enumeration value="604"/>
						<xsd:enumeration value="1520"/>
						<xsd:enumeration value="700"/>
						<xsd:enumeration value="800"/>
						<xsd:enumeration value="801"/>
						<xsd:enumeration value="900"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="VATBox">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Country"/>
				<xsd:element ref="Description"/>
			</xsd:sequence>
			<xsd:attribute name="code" type="xsd:string"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="VATSimple">
		<xsd:annotation>
			<xsd:documentation>
				The simple variant of VAT can be used in transactions for information purposes.
				VAT codes can only be imported when they comply with the complex type VAT.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="Description" minOccurs="0"/>
			<xsd:element ref="Translations" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
		<xsd:attribute name="code" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="VAT">
		<xsd:complexContent>
			<xsd:extension base="VATSimple">
				<xsd:sequence minOccurs="0">
					<xsd:element ref="Percentage" minOccurs="0"/>
					<xsd:element name="EUSalesListing" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>N=No listing, L=Listing goods, S=Listing services, T=Triangulation</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:token">
								<xsd:enumeration value="N"/>
								<xsd:enumeration value="L"/>
								<xsd:enumeration value="S"/>
								<xsd:enumeration value="T"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="Intrastat" type="xsd:boolean" minOccurs="0"/>
					<xsd:element name="IntrastatType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>
								N=Empty, L=Goods, S=Services. Only used in France legislation.
								EUSalesListing and Intrastat tag will be ignored for France and IntrastatType will be replacing them.
							</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:token">
								<xsd:enumeration value="N"/>
								<xsd:enumeration value="L"/>
								<xsd:enumeration value="S"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="ExcludeVATListing" type="xsd:boolean" minOccurs="0"/>
					<xsd:element name="VatPartialRatio" type="xsd:integer" minOccurs="0"/>
					<xsd:element name="VatDocType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>P=Purchase invoice, F=Freelance invoice, E=Expense voucher</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:token">
								<xsd:enumeration value="P"/>
								<xsd:enumeration value="F"/>
								<xsd:enumeration value="E"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="CalculationBasis" type="xsd:integer" minOccurs="0"/>
					<xsd:element name="GLToPay" type="typeGLAccount" minOccurs="0"/>
					<xsd:element name="GLToClaim" type="typeGLAccount" minOccurs="0"/>
					<xsd:element name="GLToPayProvision" type="typeGLAccount" minOccurs="0"/>
					<xsd:element name="GLToClaimProvision" type="typeGLAccount" minOccurs="0"/>
					<xsd:element name="GLDiscountSales" type="typeGLAccount" minOccurs="0"/>
					<xsd:element name="GLDiscountPurchase" type="typeGLAccount" minOccurs="0"/>
					<xsd:element ref="Account" minOccurs="0"/>
					<xsd:element ref="VATBoxLink" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element name="VATPercentages" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="VATPercentage" maxOccurs="unbounded">
									<xsd:complexType>
										<xsd:sequence minOccurs="0">
											<xsd:element ref="StartDate" minOccurs="0"/>
											<xsd:element ref="EndDate" minOccurs="0"/>
										</xsd:sequence>
										<xsd:attribute name="percentage" type="typePercentage" use="required"/>
										<xsd:attribute name="LineNumber" type="xsd:integer" use="required"/>
										<xsd:attribute name="ExtraDutyPercentage" type="typePercentage" use="optional"/>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="vatmargin" type="xsd:boolean"/>
				<xsd:attribute name="type">
					<xsd:annotation>
						<xsd:documentation>E=Excluding, I=Including, N=No vat, B= VAT 0%</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="E"/>
							<xsd:enumeration value="I"/>
							<xsd:enumeration value="N"/>
							<xsd:enumeration value="B"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="charged" type="xsd:boolean"/>
				<xsd:attribute name="vattransactiontype">
					<xsd:annotation>
						<xsd:documentation>P=Purchase, S=Sales, B=Both</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:token">
							<xsd:enumeration value="P"/>
							<xsd:enumeration value="S"/>
							<xsd:enumeration value="B"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="country" type="xsd:string" />
				<xsd:attribute name="blocked" type="xsd:boolean"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="VAT" type="VAT"/>
	<xsd:element name="VATs">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="VAT" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="VATBoxes">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="VATBox" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

