				<xsd:element ref="Translations" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="EndDate" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute ref="code"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Costcenters">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Costcenter" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Costcenter">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Description"/>
				<xsd:element ref="Translations" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="EndDate" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute ref="code"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ShippingMethods">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ShippingMethod" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ShippingMethod">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Description"/>
			</xsd:sequence>
			<xsd:attribute ref="code"/>
