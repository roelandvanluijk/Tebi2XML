			<xsd:sequence minOccurs="0">
				<xsd:element name="Age" type="xsd:int" minOccurs="0"/>
				<xsd:element name="YearsInService" type="xsd:int" minOccurs="0"/>
				<xsd:element name="Step" type="xsd:string" minOccurs="0"/>
				<xsd:element name="PeriodAmount" type="xsd:float" minOccurs="1"/>
				<xsd:element name="HourlyAmount" type="xsd:float" minOccurs="0"/>
