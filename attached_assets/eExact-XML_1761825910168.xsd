			<xsd:element name="AgencySubType" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="EnableProRataCalculation" type="xsd:boolean" minOccurs="0"/>
			<xsd:element ref="PayrollComponentOutputs" minOccurs="0"/>
			<xsd:element ref="PayrollComponentCriteria" minOccurs="0"/>
			<xsd:element ref="PayrollComponentInputs" minOccurs="0"/>
			<xsd:element ref="PayrollSubtotalLinks" minOccurs="0"/>
