				<xsd:element ref="YourRef" minOccurs="0"/>
				<xsd:element name="OrderedBy" type="typeAccount"/>
				<xsd:element name="DeliverTo" type="typeAccount">
					<xsd:annotation>
						<xsd:documentation>Only used for export. For imported sales orders the deliver to account is set to the order account.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="DeliveryAddress" minOccurs="0"/>
				<xsd:element name="InvoiceTo" type="typeAccount"/>
				<xsd:element ref="Warehouse" minOccurs="0" />
				<xsd:element ref="PaymentCondition" minOccurs="0"/>
				<xsd:element ref="ForeignAmount" minOccurs="0"/>
				<xsd:element ref="Document" minOccurs="0"/>
				<xsd:element ref="ShippingMethod" minOccurs="0"/>
				<xsd:element name="SalesPerson">
					<xsd:annotation>
						<xsd:documentation>For imported sales orders, the sales person must exist as a user to be included on the sales order. This import will not create new users.</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="FullName" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
						<xsd:attribute name="id" type="xsd:string"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="SalesChannel" type="typeSalesChannel" minOccurs="0"/>
				<xsd:element ref="Remarks" minOccurs="0"/>
				<xsd:element ref="EntryDiscount" minOccurs="0"/>
				<xsd:element ref="TaxSchedule" minOccurs="0"/>
				<xsd:element name="PaymentReference" type="xsd:string" minOccurs="0"/>						
				<xsd:element name="Incoterm" type="typeIncoterm" minOccurs="0"/>
				<xsd:element name="SalesOrderLine" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Description" minOccurs="0"/>
							<xsd:element ref="Item"/>
							<xsd:element ref="Quantity" minOccurs="0"/>
							<xsd:element name="DeliveryDate" type="xsd:date" minOccurs="0"/>
							<xsd:element ref="Unit" minOccurs="0"/>
							<xsd:element name="UnitPrice" type="typePrice" minOccurs="0"/>
							<xsd:element name="NetPrice" type="typePrice" minOccurs="0"/>
							<xsd:element ref="ForeignAmount" minOccurs="0"/>
							<xsd:element name="DiscountPercentage" type="typePercentage" minOccurs="0"/>
							<xsd:element ref="Costcenter" minOccurs="0"/>
							<xsd:element ref="Costunit" minOccurs="0"/>
							<xsd:element ref="Note" minOccurs="0"/>
							<xsd:element name="UseDropShipment" minOccurs="0"/>
							<xsd:element name="ShopOrder" minOccurs="0"/>
							<xsd:element name="VersionNumber" minOccurs="0"/>
							<xsd:element ref="TaxSchedule" minOccurs="0"/>
						</xsd:sequence>
						<xsd:attribute name="line" type="xsd:integer" use="optional"/>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="salesordernumber" use="optional">
				<xsd:annotation>
					<xsd:documentation>The sales order number can only be used once. If a sales order with this sales order number already exists, it will either 1) be recreated if the existing sales order status is Open, or 2) be ignored in the import if there is activity on the existing sales order. When no sales order number is supplied, a new sales order with the next available sales order number will be created.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="status" use="optional">
				<xsd:annotation>
					<xsd:documentation>12=Open, 20=Partial, 21=Complete, 45=Canceled. Only used for export, imported sales orders status is always set to 12.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:token">
						<xsd:enumeration value="12"/>
						<xsd:enumeration value="20"/>
						<xsd:enumeration value="21"/>
						<xsd:enumeration value="45"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
